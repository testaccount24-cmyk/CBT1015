/* REXX */
/*******************************************************************/
/*                                                                 */
/* ProgramNAME      : RXObject  (fuer ZOS UND LUW   )              */
/* AUTHER           : ULRICH BRAEUER                               */
/* FUNKTION         : AG : Skeletons und Panels Dynamisch ausgeben */
/*                                                                 */
/*                                                                 */
/*******************************************************************/

ENV        = "TSO"
X          = MSG('OFF')
USER       = USERID()
SYSCATS    = "SYSCAT"
SYSIBMS    = "SYSIBM"
SYSIBMA    = "SYSIBMADM"
SYSPROCA   = "SYSPROC"
ADDRESS ISPEXEC 'VGET (SYSIBMS) PROFILE'
ADDRESS ISPEXEC 'VGET (SYSCATS) PROFILE'
ADDRESS ISPEXEC 'VGET (SYSIBMA) PROFILE'
ADDRESS ISPEXEC 'VGET (SYSPROCA) PROFILE'

ADDRESS ISPEXEC 'VGET (UDBTRACE) PROFILE'
IF UDBTRACE = 'ON' THEN TRACE R
ELSE                    TRACE

DROP WLine.

OBJLIB = ARG(1)   /* SKELS UND PANEL LIB */
OBJLIB = Translate(OBJLIB, " ", "'")
OBJLIB = "'" || Space(OBJLIB, 0) || "'"
ADDRESS "TSO" "ALLOC F(SKELLIB) DA("OBJLIB") OLD"

skel.0   = 23
skel.1   = "PALTERTS           PAN_PALTERTS           "
skel.2   = "PAUSW200           PAN_PAUSW200           "
skel.3   = "PAUSW20H           PAN_PAUSW20H           "
skel.4   = "PCOLCOMP           PAN_PCOLCOMP           "
skel.5   = "PCOMPAT            PAN_PCOMPAT            "
skel.6   = "PDYNPNL1           PAN_PDYNPNL1           "
skel.7   = "PLALTER            PAN_PLALTER            "
skel.8   = "PLOCLIST           PAN_PLOCLIST           "
skel.9   = "PLUPARM            PAN_PLUPARM            "
skel.10  = "PLUPARM1           PAN_PLUPARM1           "
skel.11  = "PLUWAUSW           PAN_PLUWAUSW           "
skel.12  = "PLUWAUSH           PAN_PLUWAUSH           "
skel.13  = "PLUWOBJA           PAN_PLUWOBJA           "
skel.14  = "PLUWOBJX           PAN_PLUWOBJX           "
skel.15  = "PLUWOBJH           PAN_PLUWOBJH           "
skel.16  = "PLU97911           PAN_PLU97911           "
skel.17  = "PSQLERR            PAN_PSQLERR            "
skel.18  = "PZLCAT1            PAN_PZLCAT1            "
skel.19  = "PZODIS01           PAN_PZODIS01           "
skel.20  = "PZO97911           PAN_PZO97911           "
skel.21  = "PZRECTS1           PAN_PZRECTS1           "
skel.22  = "PZREOTS1           PAN_PZREOTS1           "
skel.23  = "PACORO             PAN_PACORO             "

do skeli = 1 to skel.0
   Drop WLine.
   WLine.0  = 0
   MemName  = Word(skel.skeli, 1)
   MemProc  = Word(skel.skeli, 2)
   If MemName = "" Then Iterate
   Interpret "Call "MemProc
   Call Write_Mem
 End

MemName = "SKELVER"
WLine.0 = 1
WLine.1 = "RXOBJECT"
Call Write_Mem

ADDRESS "TSO" "FREE FI(SKELLIB)"
Return 0

WL:
if Datatype(WLine.0) <> "NUM" Then WLine.0 = 0
WLine.0 = WLine.0 + 1
WLineI  = WLine.0

WLine.WLineI = Arg(1)

Return 0

Write_Mem:
ADDRESS ISPEXEC
'LMINIT DATAID(DID) DDNAME(SKELLIB) ENQ(EXCLU)'
'LMOPEN DATAID(&DID) OPTION(OUTPUT)'

Do i = 1 to WLine.0
  P1 = WLine.i
  "LMPUT DATAID(&DID) MODE(INVAR) DATALOC(P1) DATALEN(80)"
 End

'LMMADD DATAID(&DID) MEMBER('MemName')'
'LMFREE DATAID(&DID)'

 Return 0

PAN_PALTERTS:
Z=")ATTR"
OK=WL(Z)
Z="  | type(input) intens(high)             caps(on ) pad(' ')"
OK=WL(Z)
Z="  @ type(output) intens(low ) just(left) caps(off) padc('')"
OK=WL(Z)
Z="  ? type(output)intens(high) just(left)  caps(off) padc('')"
OK=WL(Z)
Z=")BODY  EXPAND(//)"
OK=WL(Z)
Z="%-/-/- Alter   ?auswtext"
OK=WL(Z)
Z="%Command ===>_zcmd                              / / %Scroll ===>" ||,
"_AMT +          "
OK=WL(Z)
Z="+ +"
OK=WL(Z)
Z="+Name               +Old Value           +New Value           + " ||,
"               +"
OK=WL(Z)
Z="+--------------     +------------------- +------------------- + " ||,
"               +"
OK=WL(Z)
Z=")MODEL"
OK=WL(Z)
Z="@Name               @OVal                |NVal                + " ||,
"               +"
OK=WL(Z)
Z=")INIT"
OK=WL(Z)
Z=")PROC"
OK=WL(Z)
Z="&PF = .PFKEY"
OK=WL(Z)
Z="&PANELRC = 0"
OK=WL(Z)
Z="IF (.RESP = END,RETURN)"
OK=WL(Z)
Z="    .RESP = ENTER"
OK=WL(Z)
Z="     &PANELRC = 8"
OK=WL(Z)
Z="IF (&ZCMD = CAN,CANCEL)"
OK=WL(Z)
Z="   &PANELRC = 9"
OK=WL(Z)
Z=")End"
OK=WL(Z)

RETURN 0

PAN_PAUSW200:
Z=")ATTR                                                           "
OK=WL(Z)
Z=" § AREA(DYNAMIC)   SCROLL(ON) EXTEND(ON) DATAMOD(05)            "
OK=WL(Z)
Z=" @ AREA(DYNAMIC)   SCROLL(ON) EXTEND(ON) DATAMOD(05)            "
OK=WL(Z)
Z=" 01 TYPE(DATAOUT)  COLOR(RED)                                   "
OK=WL(Z)
Z=" 02 TYPE(DATAOUT)  COLOR(BLUE)                                  "
OK=WL(Z)
Z=" 03 TYPE(DATAOUT)  COLOR(GREEN)                                 "
OK=WL(Z)
Z=" 04 TYPE(DATAOUT)  COLOR(WHITE)                                 "
OK=WL(Z)
Z=" 05 TYPE(DATAIN)   PAS(OFF)      CAPS(ON)                       "
OK=WL(Z)
Z=" 06 TYPE(DATAIN)   PAS(ON)       CAPS(ON) HILITE(USCORE)        "
OK=WL(Z)
Z=" 07 TYPE(DATAIN)   COLOR(RED)    CAPS(ON) JUST(LEFT)            "
OK=WL(Z)
Z="  + TYPE(TEXT) INTENS(LOW) SKIP(ON)                             "
OK=WL(Z)
Z="  $ TYPE(TEXT) INTENS(HIGH) SKIP(ON) color(pink)                "
OK=WL(Z)
Z="  * TYPE(OUTPUT) INTENS(LOW) SKIP(ON)                           "
OK=WL(Z)
Z="  | TYPE(OUTPUT) INTENS(HIGH) SKIP(ON)                          "
OK=WL(Z)
Z=")BODY EXPAND(~~)                                                "
OK=WL(Z)
Z="%~-~ process  DB2 for zOS            ~-~                     "
OK=WL(Z)
Z="%Command ===>_ZCMD                                              " ||,
"   +            "
OK=WL(Z)
Z="+ User-ID|USER    +SSID|Ssid +LOCATION |Location   +VERSION |Ver" ||,
"sion           +"
OK=WL(Z)
Z="+ Session|lnr     +SQLID|CURSQLID       +                       " ||,
"                "
OK=WL(Z)
Z="+|Z                                                             " ||,
"      +         "
OK=WL(Z)
Z="+                                                               "
OK=WL(Z)
Z="§DYNAREA                                                        " ||,
"               §"
OK=WL(Z)
Z="+                                                               "
OK=WL(Z)
Z="+ Select %===>_Z               +  + Edit SQL%===>_Z+ Y/N        " ||,
"              + "
OK=WL(Z)
Z="+ Object %===>_Z                                                " ||,
"              + "
OK=WL(Z)
Z="+                                                               "
OK=WL(Z)
Z="%~-~  E n d e  ~-~                                              "
OK=WL(Z)
Z=")INIT                                                           "
OK=WL(Z)
Z="  .ZVARS    = '(MSGLINE AUSWAHL EDITSQL OBJECT)'                "
OK=WL(Z)
Z="  .CURSOR   = AUSWAHL                                           "
OK=WL(Z)
Z="  .HELP     = PAUSW20H                                          "
OK=WL(Z)
Z="  &USER     = &USER                                             "
OK=WL(Z)
Z="  &SSID     = &SSID                                             "
OK=WL(Z)
Z="  &LOCATION = &LOCATION                                         "
OK=WL(Z)
Z="  &EDITSQL  = &EDITSQL                                          "
OK=WL(Z)
Z="  &Object   = &Object                                           "
OK=WL(Z)
Z="  &MSGLINE  = &MSGLINE                                          "
OK=WL(Z)
Z=")PROC                                                           "
OK=WL(Z)
Z="&DYNDPTH = LVLINE(DYNAREA)                                      "
OK=WL(Z)
Z="&PF = .PFKEY                                                    "
OK=WL(Z)
Z=")END                                                            "
OK=WL(Z)

RETURN

