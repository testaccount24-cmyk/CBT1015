"               +"
OK=WL(Z)
Z="|MSGLINE                                                        " ||,
"               +"
OK=WL(Z)
Z="%-----------------------------------END-------------------------" ||,
"---------------+"
OK=WL(Z)
Z=")INIT                                                           "
OK=WL(Z)
Z="  .CURSOR   = DYNAREA                                           "
OK=WL(Z)
Z="  .CSRPOS   = &DYNPOS                                           "
OK=WL(Z)
Z="  &USER     = &USER                                             "
OK=WL(Z)
Z="  &SSID     = &SSID                                             "
OK=WL(Z)
Z="  &LOCATION = &LOCATION                                         "
OK=WL(Z)
Z="  &EDITSQL  = &EDITSQL                                          "
OK=WL(Z)
Z="  &Object   = &Object                                           "
OK=WL(Z)
Z="  &MSGLINE  = &MSGLINE                                          "
OK=WL(Z)
Z="  &PFTASTEN = &PFTASTEN                                         "
OK=WL(Z)
Z=")PROC                                                           "
OK=WL(Z)
Z="&PF      = .PFKEY                                               "
OK=WL(Z)
Z="&RESP    = .RESP                                                "
OK=WL(Z)
Z="&DYNKEY  = .PFKEY                                               "
OK=WL(Z)
Z="&DYNPOS  = .CSRPOS                                              "
OK=WL(Z)
Z="&DYNDPTH = LVLINE(DYNAREA)                                      "
OK=WL(Z)
Z=")END                                                            "
OK=WL(Z)

RETURN

