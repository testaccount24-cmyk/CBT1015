Z=" @ AREA(DYNAMIC)   SCROLL(ON) EXTEND(ON) DATAMOD(05)            "
OK=WL(Z)
Z=" 01 TYPE(DATAOUT)  COLOR(RED)   SKIP(ON)                        "
OK=WL(Z)
Z=" 02 TYPE(DATAOUT)  COLOR(BLUE)  SKIP(ON)                        "
OK=WL(Z)
Z=" 03 TYPE(DATAOUT)  COLOR(GREEN) SKIP(ON)                        "
OK=WL(Z)
Z=" 04 TYPE(DATAOUT)  COLOR(WHITE) SKIP(ON)                        "
OK=WL(Z)
