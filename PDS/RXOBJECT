OK=WL(Z)
Z=" $  TYPE(DATAOUT)           COLOR(YELLOW)                       "
OK=WL(Z)
Z=" | TYPE(OUTPUT) COLOR(YELLOW)  SKIP(ON)                         "
OK=WL(Z)
Z=")BODY                                                           "
OK=WL(Z)
Z="%------------------  EXAMPLE FOR USING A DYNAMIC AREA  ---------" ||,
"---------------%"
OK=WL(Z)
Z="%COMMAND ===>_ZCMD                                            %S" ||,
"CROLL ===>_AMT +"
OK=WL(Z)
