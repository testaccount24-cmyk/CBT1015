PAN_PACORO:
Z=")ATTR"
OK=WL(Z)
Z=" + TYPE(TEXT)   INTENS(LOW)"
OK=WL(Z)
Z=" # TYPE(TEXT)   INTENS(HIGH)"
OK=WL(Z)
Z=" ? TYPE(TEXT)   INTENS(HIGH) PAD(_)"
OK=WL(Z)
Z=" _ TYPE(INPUT)  INTENS(LOW) PAD(_)"
OK=WL(Z)
Z=" | TYPE(INPUT)  INTENS(LOW)"
OK=WL(Z)
Z=" % TYPE(OUTPUT) INTENS(HIGH)"
OK=WL(Z)
Z=")BODY  EXPAND(//)"
OK=WL(Z)
Z="%COMMIT OR ROLLBACK -/-/-  +"
OK=WL(Z)
Z="+Command ===>_zcmd"
OK=WL(Z)
Z="+"
OK=WL(Z)
Z="# WAS ISS :"
OK=WL(Z)
Z="+"
OK=WL(Z)
Z="+         AUTOCOMMIT WAS NO  now enter  COMMIT OR ROLLBACK"
OK=WL(Z)
Z="+                         "
OK=WL(Z)
Z="+"
OK=WL(Z)
Z="+         * COMMIT   (SAVE)  CHANGES"
OK=WL(Z)
Z="+         * ROLLBACK (ERASE) CHANGES"
OK=WL(Z)
Z="+"
OK=WL(Z)
Z="#    NEXT ACTION : _COMROL  #  ENTER COMMIT OR ROLLBACK"
OK=WL(Z)
Z="+"
OK=WL(Z)
Z="+"
OK=WL(Z)
Z="+"
OK=WL(Z)
Z="+"
OK=WL(Z)
Z="+"
OK=WL(Z)
Z="+"
OK=WL(Z)
Z="+"
OK=WL(Z)
Z="+PRESS:  ENTER=GO PF3=COMMIT     "
OK=WL(Z)
Z="+"
OK=WL(Z)
Z=")INIT"
OK=WL(Z)
Z="  &ZCMD = &Z"
OK=WL(Z)
Z=")REINIT"
OK=WL(Z)
Z="  REFRESH(*)"
OK=WL(Z)
Z="  .CURSOR = COMROL              /* SET CURSOR TO INPUT LINE  */"
OK=WL(Z)
Z=")PROC"
OK=WL(Z)
Z="  &PFTASTE  = .PFKEY"
OK=WL(Z)
Z="  &PFINHALT = TRUNC(&PFTASTE,2)"
OK=WL(Z)
Z="  &PFINHALT = .TRAIL"
OK=WL(Z)
Z="  &PFINHALT = PFK(&PFINHALT)"
OK=WL(Z)
Z="  &PANELRC = 0"
OK=WL(Z)
Z=""
OK=WL(Z)
Z="  IF (.RESP = END,RETURN)"
OK=WL(Z)
Z="    &PANELRC = 4"
OK=WL(Z)
Z="    .RESP    = ENTER"
OK=WL(Z)
Z="  IF (&PFINHALT = RETURN)"
OK=WL(Z)
Z="    &PANELRC = 8"
OK=WL(Z)
Z="    .RESP    = ENTER"
OK=WL(Z)
Z="  IF (&COM  = CAN,CANCEL)"
OK=WL(Z)
Z="    &PANELRC = 9"
OK=WL(Z)
Z="    .RESP    = ENTER"
OK=WL(Z)
Z="  &VZCMD1 = TRUNC(&COM,' ')"
OK=WL(Z)
Z="  &VZCMD2 = .TRAIL"
OK=WL(Z)
Z=""
OK=WL(Z)
Z=")END"
