LOCKS D         OBJECT1 = STRIP(AG)
LOCKS D         OBJECT2 = ""
LOCKS D         DB2_LIST_OBJ  = SYSIBMA".LOCKS_HELD"
LOCKS D         AUSWAHLOK = 'Y'

LOCKS APPLI     OBJECT1 = STRIP(AG)
LOCKS APPLI     OBJECT2 = ""
LOCKS APPLI     AUSWAHLOK = 'Y'

LOCKS APPL      EINGABE = 'D'
LOCKS APPL      OBJECT1 = STRIP(AG)
LOCKS APPL      OBJECT2 = ""
LOCKS APPL      DB2_LIST_OBJ  = SYSIBMA".SNAPAPPL"
LOCKS APPL      AUSWAHLOK = 'Y'

LOCKHOLD APPLI  OBJECT1   =  STRIP(HAPP)
LOCKHOLD APPLI  OBJECT2   =  ''
LOCKHOLD APPLI  AUSWAHLOK = 'Y'

LOCKHOLD HAPP   EINGABE   =  'APPLI'
LOCKHOLD HAPP   OBJECT1   =  STRIP(HAPP)
LOCKHOLD HAPP   OBJECT2   =  ''
LOCKHOLD HAPP   AUSWAHLOK = 'Y'

LOCKHOLD RAPP   EINGABE   =  'APPLI'
LOCKHOLD RAPP   OBJECT1   =  STRIP(RAPP)
LOCKHOLD RAPP   OBJECT2   =  ''
LOCKHOLD RAPP   AUSWAHLOK = 'Y'

LOCKHOLD RLOCKS EINGABE   =  'LOCKS'
LOCKHOLD RLOCKS OBJECT1   =  ''
LOCKHOLD RLOCKS OBJECT2   =  STRIP(RAPP)
LOCKHOLD RLOCKS AUSWAHLOK = 'Y'

LOCKHOLD HLOCKS EINGABE   =  'LOCKS'
LOCKHOLD HLOCKS OBJECT1   =  ''
LOCKHOLD HLOCKS OBJECT2   =  STRIP(HAPP)
LOCKHOLD HLOCKS AUSWAHLOK = 'Y'

LOCKHOLD D      OBJECT1   =  STRIP(LN)
LOCKHOLD D      OBJECT2   =  STRIP(HAPP) || '.' STRIP(RAPP)
LOCKHOLD D      DB2_LIST_OBJ = SYSIBMA".MON_LOCKWAITS"
LOCKHOLD D      AUSWAHLOK = 'Y'

APPLI D         OBJECT1 = STRIP(AG)
APPLI D         OBJECT2 = ""
APPLI D         DB2_LIST_OBJ  = SYSIBMA".SNAPAPPL_INFO"
APPLI D         AUSWAHLOK = 'Y'

APPLI APPL      EINGABE = 'D'
APPLI APPL      OBJECT1 = STRIP(AG)
APPLI APPL      OBJECT2 = ""
APPLI APPL      DB2_LIST_OBJ  = SYSIBMA".SNAPAPPL"
APPLI APPL      AUSWAHLOK = 'Y'

APPLI LOCKS     OBJECT1 = ""
APPLI LOCKS     OBJECT2 = STRIP(AG)
APPLI LOCKS     AUSWAHLOK = 'Y'

FEAT D          OBJECT1 = STRIP(NM)
FEAT D          OBJECT2 = ""
FEAT D          DB2_LIST_OBJ  = SYSIBMA".ENV_FEATURE_INFO"
FEAT D          AUSWAHLOK = 'Y'

PRIV D          OBJECT1 = STRIP(OS) || "." || STRIP(ON)
PRIV D          OBJECT2 = AI
PRIV D          DB2_LIST_OBJ  = SYSIBMA".PRIVILEGES"
PRIV D          AUSWAHLOK = 'Y'

ST D            OBJECT1 = STRIP(CR) || '.' || STRIP(NM) || ' ' VW
ST D            OBJECT2 = SM SC SQ
ST D            IF STRIP(VW) = "-" THEN OBJECT2 = ""
ST D            DB2_LIST_OBJ  = SYSCATS".STATEMENTS"
ST D            AUSWAHLOK = 'Y'

SC   D          OBJECT1   =  STRIP(SC) || '.' || STRIP(SN)
SC   D          OBJECT2   =  ''
SC   D          DB2_LIST_OBJ = SYSCATS".SCHEMATA"
SC   D          AUSWAHLOK = 'Y'

SC   TB         OBJECT1   =  STRIP(SN) || '.*'
SC   TB         OBJECT2   =  ''
SC   TB         AUSWAHLOK = 'Y'

SC   IX         OBJECT1   =  STRIP(SN) || '.*'
SC   IX         OBJECT2   =  ''
SC   IX         AUSWAHLOK = 'Y'

SC   VW         OBJECT1   =  STRIP(SN) || '.*'
SC   VW         OBJECT2   =  ''
SC   VW         AUSWAHLOK = 'Y'

SC   DROP       OBJECT1   =  STRIP(SN)
SC   DROP       OBJECT2 = ""
SC   DROP       CALL AUFRUF_LUWA1915
SC   DROP       AUSWAHLOK = 'N'

SC DDL          OBJECT1 = STRIP(SC) || '.' || STRIP(SN)
SC DDL          OBJECT2 = ""
SC DDL          CALL AUFRUF_LUWA1920
SC DDL          AUSWAHLOK = 'N'

SC HDDL         OBJECT1 = STRIP(SC) || '.' || STRIP(SN)
SC HDDL         OBJECT2 = ""
SC HDDL         CALL AUFRUF_LUWA1920
SC HDDL         AUSWAHLOK = 'N'

MEM  D          OBJECT1   =  '*.' || STRIP(NR)
MEM  D          DB2_LIST_OBJ  = "DB_MEMBERS"
MEM  D          AUSWAHLOK = 'Y'

PERM D          OBJECT1   =  STRIP(CR) || '.' || STRIP(NM)
PERM D          DB2_LIST_OBJ  = SYSCATS".CONTROLS"
PERM D          AUSWAHLOK = 'Y'

PERM TB         OBJECT1   =  STRIP(TC) || '.' || STRIP(TN)
PERM TB         AUSWAHLOK = 'Y'

PERM VW         OBJECT1   =  STRIP(TC) || '.' || STRIP(TN)
PERM VW         AUSWAHLOK = 'Y'

PERM CO         OBJECT1   =  STRIP(CON)
PERM CO         OBJECT2   =  STRIP(TC) || '.' || STRIP(TN)
PERM CO         AUSWAHLOK = 'Y'
PERM CO         IF COTY = 'R' THEN AUSWAHLOK = 'N'

PERM ACTIVATE   OBJECT1   =  STRIP(TC) || '.' || STRIP(TN)
PERM ACTIVATE   OBJECT2   =  ""
PERM ACTIVATE   OBJECT3   =  COTY
PERM ACTIVATE   CALL AUFRUF_LUWA1915
PERM ACTIVATE   AUSWAHLOK = 'N'

PERM DEACTIVATE   OBJECT1   =  STRIP(TC) || '.' || STRIP(TN)
PERM DEACTIVATE   OBJECT2   =  ""
PERM DEACTIVATE   OBJECT3   =  COTY
PERM DEACTIVATE   CALL AUFRUF_LUWA1915
PERM DEACTIVATE   AUSWAHLOK = 'N'

PERM ENABLE     OBJECT1   =  STRIP(CR) || '.' || STRIP(NM)
PERM ENABLE     OBJECT2   =  ""
PERM ENABLE     OBJECT3   =  COTY
PERM ENABLE     CALL AUFRUF_LUWA1915
PERM ENABLE     AUSWAHLOK = 'N'

PERM DISABLE    OBJECT1   =  STRIP(CR) || '.' || STRIP(NM)
PERM DISABLE    OBJECT2   =  ""
PERM DISABLE    OBJECT3   =  COTY
PERM DISABLE    CALL AUFRUF_LUWA1915
PERM DISABLE    AUSWAHLOK = 'N'

PERM REGENERATE OBJECT1   =  STRIP(CR) || '.' || STRIP(NM)
PERM REGENERATE OBJECT2   =  ""
PERM REGENERATE OBJECT3   =  COTY
PERM REGENERATE CALL AUFRUF_LUWA1915
PERM REGENERATE AUSWAHLOK = 'N'

PERM DROP       OBJECT1    = CR || "." || NM
PERM DROP       OBJECT2 = ""
PERM DROP       CALL AUFRUF_LUWA1915
PERM DROP       AUSWAHLOK = 'N'

VER  D          OBJECT1   =  '*.' || STRIP(VTS)
VER  D          DB2_LIST_OBJ  = SYSIBMS".SYSVERSIONS"
VER  D          AUSWAHLOK = 'Y'

