OK=WL(Z)
Z="+ User-ID|USER    +SSID|Ssid +LOCATION |Location   +VERSION |" || ,
  "Version           +"
OK=WL(Z)
Z="+|Z                                                                +"
OK=WL(Z)
Z="+                                                                   "
OK=WL(Z)
Z="§DYNAREA                                                     " || ,
  "                  §"
OK=WL(Z)
Z="+                                                                   "
OK=WL(Z)
Z="+ Auswahl%===>_Z               +  + Edit SQL%===>_Z+ Y/N           +"
OK=WL(Z)
Z="+ Object %===>_Z                                             " || ,
  "Version        +   "
OK=WL(Z)
