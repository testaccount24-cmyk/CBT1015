OK=WL(Z)
Z=")IF #TS EQ *  THEN )DO                                              "
OK=WL(Z)
Z="  -DIS  UTILITY(QUIESCE.TABLESP )                                   "
OK=WL(Z)
Z="  -TERM UTILITY(QUIESCE.TABLESP )                                   "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
