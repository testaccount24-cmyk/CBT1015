OK=WL(Z)
Z="//SYSIN    DD  DUMMY                                                "
OK=WL(Z)
Z="//SYSTSIN  DD  *                                                    "
OK=WL(Z)
Z="  DSN SYSTEM(#SSID.)                                                "
OK=WL(Z)
Z="  -DIS  UTILITY(REPLVL.#TS)                                         "
OK=WL(Z)
Z="  -TERM UTILITY(REPLVL.#TS)                                         "
OK=WL(Z)
