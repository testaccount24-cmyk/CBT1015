OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="//SYSIN DD *                                                        "
OK=WL(Z)
Z="-- OPTIONS EVENT(ITEMERROR,SKIP)                                    "
OK=WL(Z)
Z="  CHECK INDEX LIST CHECKIDX                                         "
OK=WL(Z)
Z="        WORKDDN  (TSYSUT1)                                          "
OK=WL(Z)
Z="        SORTDEVT #WORK                                              "
OK=WL(Z)
Z="        SORTNUM 4                                                   "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="// IF (CHECKIX.RC GT 4 OR CHECKIX.ABEND = TRUE) THEN                "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
