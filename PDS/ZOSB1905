SKEL_SKREP01:
Z=")DEFAULT  )#?ü<~>                                                   "
OK=WL(Z)
Z="//REPLVL  EXEC PGM=DSNUTILB,                                        "
OK=WL(Z)
Z=")SEL #TS EQ *                                                       "
OK=WL(Z)
Z="//         PARM=(#SSID,'REP.TABLESP')                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #TS NE *                                                       "
OK=WL(Z)
Z="//         PARM=(#SSID,'REP.#TS')                                   "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSPRINT  DD SYSOUT=*                                             "
OK=WL(Z)
Z="//UTPRINT   DD SYSOUT=*                                             "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="//SYSIN DD *                                                        "
OK=WL(Z)
Z="--OPTIONS PREVIEW                                                   "
OK=WL(Z)
Z="--OPTIONS EVENT(ITEMERROR,SKIP)                                     "
OK=WL(Z)
Z=")IF #USEDOT  = Y THEN )DO                                           "
OK=WL(Z)
Z=")DOT #TSLISTE                                                       "
OK=WL(Z)
Z=")SEL #LEVEL   = Y                                                   "
OK=WL(Z)
Z="    REPAIR LEVELID  TABLESPACE #DBNAME..#TSNAME.                    "
OK=WL(Z)
Z=")SEL #part NE 0                                                     "
OK=WL(Z)
Z="           PART #part                                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #VERS    = Y                                                   "
OK=WL(Z)
Z="    REPAIR VERSIONS TABLESPACE #DBNAME..#TSNAME.                    "
OK=WL(Z)
Z=")SEL #part NE 0                                                     "
OK=WL(Z)
Z="           PART #part                                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #CAT     = Y                                                   "
OK=WL(Z)
Z="    REPAIR CATALOG  TABLESPACE #DBNAME..#TSNAME.                    "
OK=WL(Z)
Z=")SEL #part NE 0                                                     "
OK=WL(Z)
Z="           PART #part                                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #SETTYPE NE #BLANK                                             "
OK=WL(Z)
Z=")SEL #TYPE    NE INDEX                                              "
OK=WL(Z)
Z="    REPAIR SET TABLESPACE #DBNAME..#TSNAME.  #SETTYPE               "
OK=WL(Z)
Z=")SEL #part NE 0                                                     "
OK=WL(Z)
Z="           PART #part                                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #TYPE    EQ INDEX                                              "
OK=WL(Z)
Z="    REPAIR SET INDEX #CREATOR..#NAME.  #SETTYPE                     "
OK=WL(Z)
Z=")SEL #part NE 0                                                     "
OK=WL(Z)
Z="           PART #part                                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z=")SEL #LEVEL   = Y                                                   "
OK=WL(Z)
Z="    REPAIR LEVELID  TABLESPACE #DB..#TS.                            "
OK=WL(Z)
Z=")SEL #part NE 0                                                     "
OK=WL(Z)
Z="           PART #part                                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #VERS    = Y                                                   "
OK=WL(Z)
Z="    REPAIR VERSIONS TABLESPACE #DB..#TS.                            "
OK=WL(Z)
Z=")SEL #part NE 0                                                     "
OK=WL(Z)
Z="           PART #part                                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #CAT     = Y                                                   "
OK=WL(Z)
Z="    REPAIR CATALOG  TABLESPACE #DB..#TS.                            "
OK=WL(Z)
Z=")SEL #part NE 0                                                     "
OK=WL(Z)
Z="           PART #part                                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #SETTYPE NE #BLANK                                             "
OK=WL(Z)
Z=")SEL #TYPE    NE INDEX                                              "
OK=WL(Z)
Z="    REPAIR SET #TYPE #DB..#TS.  #SETTYPE                            "
OK=WL(Z)
Z=")SEL #part NE 0                                                     "
OK=WL(Z)
Z="           PART #part                                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #TYPE    EQ INDEX                                              "
OK=WL(Z)
Z="    REPAIR SET #TYPE #CREATOR..#NAME.  #SETTYPE                     "
OK=WL(Z)
Z=")SEL #part NE 0                                                     "
OK=WL(Z)
Z="           PART #part                                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="// IF (REPLVL.RC GT 4 OR REPLVL.ABEND = TRUE) THEN                  "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
OK=WL(Z)
