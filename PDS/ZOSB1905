/* REXX */
/*******************************************************************/
/*                                                                 */
/* ProgramNAME      : ZOSB1905  (fuer ZOS V11 Nr 905)              */
/* AUTHER           : ULRICH BRAEUER                               */
/* FUNKTION         : AG : Skeletons Dynamisch ausgeben           */
/*                                                                 */
/*                                                                 */
/*******************************************************************/

ENV        = "TSO"
X          = MSG('OFF')
USER       = USERID()
SYSCATS    = "SYSCAT"
SYSIBMS    = "SYSIBM"
SYSIBMA    = "SYSIBMADM"
SYSPROCA   = "SYSPROC"
ADDRESS ISPEXEC 'VGET (SYSIBMS) PROFILE'
ADDRESS ISPEXEC 'VGET (SYSCATS) PROFILE'
ADDRESS ISPEXEC 'VGET (SYSIBMA) PROFILE'
ADDRESS ISPEXEC 'VGET (SYSPROCA) PROFILE'

ADDRESS ISPEXEC 'VGET (UDBTRACE) PROFILE'
IF UDBTRACE = 'ON' THEN TRACE R
ELSE                    TRACE

DROP WLine.

PARSE SOURCE SRC.1 SRC.2 SRC.3 SRC.4 SRC.5 SRC.6 SRC.7 SRC.8 SRC.9
OBJLIB  = ARG(1)   /* SKELS UND PANEL LIB */
OBJLIB1 = Strip(Translate(OBJLIB, " ", "'") )
OBJLIB  = "'" || Space(OBJLIB1, 0) || "'"
SKELVER = "'"OBJLIB1 || "(SKELVER)'"

ADDRESS "TSO" "ALLOC F(OBJLIB) DA("OBJLIB") OLD"
Call GetMem
ADDRESS "TSO" "FREE  F(OBJLIB) "

If SKELV     = SRC.3  Then do
   /* Gleiche Version dann Return */
   Return
 End

ADDRESS "TSO" "ALLOC F(OBJLIB) DA("OBJLIB") OLD"

MemName = "SKELVER"
WLine.0 = 1
WLine.1 = SRC.3
Call Write_Mem


skel.0   = 52
skel.1   = "SKALTS9            SKEL_SKALTS9           "
skel.2   = "SKCHKDT1           SKEL_SKCHKDT1          "
skel.3   = "SKCHKIX1           SKEL_SKCHKIX1          "
skel.4   = "SKCPYCPY           SKEL_SKCPYCPY          "
skel.5   = "SKCPYMER           SKEL_SKCPYMER          "
skel.6   = "SKCPYTS1           SKEL_SKCPYTS1          "
skel.7   = "SKDIAG             SKEL_SKDIAG            "
skel.8   = "SKCPYIX1           SKEL_SKCPYIX1          "
skel.9   = "SKICINFO           SKEL_SKICINFO          "
skel.10  = "SKJU003            SKEL_SKJU003           "
skel.11  = "SKJU004            SKEL_SKJU004           "
skel.12  = "SKLOGP             SKEL_SKLOGP            "
skel.13  = "SKMODALL           SKEL_SKMODALL          "
skel.14  = "SKMODHIS           SKEL_SKMODHIS          "
skel.15  = "SKMODREC           SKEL_SKMODREC          "
skel.16  = "SKPRNT             SKEL_SKPRNT            "
skel.17  = "SKQUIES1           SKEL_SKQUIES1          "
skel.18  = "SKRBPG1            SKEL_SKRBPG1           "
skel.19  = "SKBPG1             SKEL_SKBPG1            "
skel.20  = "SKRBPL1            SKEL_SKRBPL1           "
skel.21  = "SKREBIX1           SKEL_SKREBIX1          "
skel.22  = "SKRECOV1           SKEL_SKRECOV1          "
skel.23  = "SKRECOV2           SKEL_SKRECOV2          "
skel.24  = "SKRECOV3           SKEL_SKRECOV3          "
skel.25  = "SKREOIX1           SKEL_SKREOIX1          "
skel.26  = "SKREOTS1           SKEL_SKREOTS1          "
skel.27  = "SKREOTS2           SKEL_SKREOTS2          "
skel.28  = "SKREPLVL           SKEL_SKREPLVL          "
skel.29  = "SKREPTS1           SKEL_SKREPTS1          "
skel.30  = "SKREPTS2           SKEL_SKREPTS2          "
skel.31  = "SKREP01            SKEL_SKREP01           "
skel.32  = "SKRSIX1            SKEL_SKRSIX1           "
skel.33  = "SKRSTS1            SKEL_SKRSTS1           "
skel.34  = "SKRSTS2            SKEL_SKRSTS2           "
skel.35  = "SKSDMP             SKEL_SKSDMP            "
skel.36  = "SKSTEPLI           SKEL_SKSTEPLI          "
skel.37  = "SKSTOSPA           SKEL_SKSTOSPA          "
skel.38  = "SKTEP2             SKEL_SKTEP2            "
skel.39  = "SKTEP4             SKEL_SKTEP4            "
skel.40  = "SKTIAD             SKEL_SKTIAD            "
skel.41  = "SKTIAUL            SKEL_SKTIAUL           "
skel.42  = "SKUNLIC            SKEL_SKUNLIC           "
skel.43  = "SKUNLTB            SKEL_SKUNLTB           "
skel.44  = "SKUNLTS            SKEL_SKUNLTS           "
skel.45  = "SK1COPY1           SKEL_SK1COPY1          "
skel.46  = "SK1COPY2           SKEL_SK1COPY2          "
skel.47  = "SK1LOGP            SKEL_SK1LOGP           "
skel.48  = "SK1PRINT           SKEL_SK1PRINT          "
skel.49  = "PDYNPNL1           PAN_PDYNPNL1           "
skel.50  = "PZO97911           PAN_PZO97911           "
skel.51  = "SKREPCAT           SKEL_SKREPCAT          "
skel.52  = "SKALTS1            SKEL_SKALTS1           "

do skeli = 1 to skel.0
   Drop WLine.
   WLine.0  = 0
   MemName  = Word(skel.skeli, 1)
   MemProc  = Word(skel.skeli, 2)
   If MemName = "" Then Iterate
   Interpret "Call "MemProc
   Call Write_Mem
 End

ADDRESS "TSO" "FREE FI(OBJLIB)"
Return 0

WL:
if Datatype(WLine.0) <> "NUM" Then WLine.0 = 0
WLine.0 = WLine.0 + 1
WLineI  = WLine.0

WLine.WLineI = Arg(1)

Return 0

GetMem:
ADDRESS ISPEXEC
SKELV  = ""
pdsmbr = 'SKELVER'
'LMINIT DATAID(DID) DDNAME(OBJLIB) ENQ(EXCLU)'
'LMOPEN DATAID(&DID) OPTION(INPUT)'
'LMMLIST DATAID(&DID) OPTION(LIST) MEMBER(PDSMBR) STATS(YES)'

'LMMFIND DATAID(&DID) MEMBER('PDSMBR')',
    'LRECL(reclen) RECFM(recfmvr)',
    'STATS(NO)'

'lmget dataid(&DID) mode(invar) dataloc(SKELV) ',
                   ' datalen(datavar) maxlen(80)'

'LMFREE DATAID(&DID)'

 Return 0


Write_Mem:
ADDRESS ISPEXEC
'LMINIT DATAID(DID) DDNAME(OBJLIB) ENQ(EXCLU)'
'LMOPEN DATAID(&DID) OPTION(OUTPUT)'
'LMMDEL DATAID(&DID) MEMBER('MemName')'

Do i = 1 to WLine.0
  P1 = WLine.i
  "LMPUT DATAID(&DID) MODE(INVAR) DATALOC(P1) DATALEN(80)"
 End

'LMMADD DATAID(&DID) MEMBER('MemName')'
'LMFREE DATAID(&DID)'
'LMCLOSE DATAID(&DID)'
'LMFREE DATAID(&DID)'

 Return 0

SKEL_SKALTS9:
Z=")DEFAULT  )#?ü<~>                                                   "
OK=WL(Z)
Z=")SEL #JOB EQ Y                                                      "
OK=WL(Z)
Z="//ALTERTS EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSTSPRT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSPRINT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSUDUMP DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSTSIN  DD  *                                                    "
OK=WL(Z)
Z="  DSN SYSTEM(#SSID.)                                                "
OK=WL(Z)
Z=")SEL #STOPTS = Y                                                    "
OK=WL(Z)
Z="     -STOP  DATABASE(#DBNAME) SPACENAM(#TSNAME) AT(COMMIT)          "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="      RUN  PROGRAM(DSNTEP4) -                                       "
OK=WL(Z)
Z="           PLAN(DSNTEP4)    -                                       "
OK=WL(Z)
Z=")SEL #RUNLIB NE #BLANK                                              "
OK=WL(Z)
Z="           LIB('#RUNLIB')   -                                       "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="           PARMS('/ALIGN(MID)')                                     "
OK=WL(Z)
Z=")SEL #STOPTS = Y                                                    "
OK=WL(Z)
Z="     -START DATABASE(#DBNAME) SPACENAM(#TSNAME) ACCESS(RW)          "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="   END                                                              "
OK=WL(Z)
Z="//SYSIN DD *                                                        "
OK=WL(Z)
Z="--##SET ROWS_FETCH -1                                               "
OK=WL(Z)
Z="--##SET ROWS_OUT   -1                                               "
OK=WL(Z)
Z="--##SET TOLWARN    YES                                              "
OK=WL(Z)
Z="--##SET TERMINATOR ;                                                "
OK=WL(Z)
Z="--##SET TOLARTHWRN YES                                              "
OK=WL(Z)
Z="--##SET MAXERRORS  -1                                               "
OK=WL(Z)
Z="--##SET MULT_FETCH 1000                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #STMT EQ Y                                                     "
OK=WL(Z)
Z="  #SQLSTMT                                                          "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)

RETURN

