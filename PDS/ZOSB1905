PAN_PSQLERR:
Z=")ATTR DEFAULT(%+_)                                                  "
OK=WL(Z)
Z="   % TYPE(TEXT  ) INTENS(HIGH) SKIP(ON)                             "
OK=WL(Z)
Z="   + TYPE(TEXT  ) INTENS(LOW ) SKIP(ON)                             "
OK=WL(Z)
Z="   ¢ TYPE(TEXT  ) INTENS(HIGH) SKIP(ON)  HILITE(&REV1)              "
OK=WL(Z)
Z="   _ TYPE(INPUT)  INTENS(HIGH) CAPS(ON ) JUST(LEFT ) HILITE(USCORE) "
OK=WL(Z)
Z="   @ TYPE(INPUT)  INTENS(HIGH) CAPS(ON ) JUST(RIGHT) PAD(' ')       "
OK=WL(Z)
Z="     HILITE(USCORE)                                                 "
OK=WL(Z)
Z="   | TYPE(OUTPUT) INTENS(LOW ) CAPS(OFF) JUST(LEFT) PAD(' ')        "
OK=WL(Z)
Z="   ° TYPE(OUTPUT) INTENS(HIGH) CAPS(OFF) JUST(RIGHT) PAD(' ')       "
OK=WL(Z)
Z=")BODY EXPAND(//)                                                    "
OK=WL(Z)
Z="/-/  Text fuer SQLCODE anzeigen (DSNTIAR) %/-/                      "
OK=WL(Z)
Z="¢=>_ZCMD                                                            "
OK=WL(Z)
Z="+SQLCODE        :_Z        +                                        "
OK=WL(Z)
Z="+                                                                   "
OK=WL(Z)
Z="+SQLMSG  Z1     :|Z                                                 "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="+                                                                   "
OK=WL(Z)
Z="+                                                                   "
OK=WL(Z)
Z="+                                                                   "
OK=WL(Z)
Z="+                                                                   "
OK=WL(Z)
Z="+                                                                   "
OK=WL(Z)
Z="+                                                                   "
OK=WL(Z)
Z="+                                                                   "
OK=WL(Z)
Z="¢F3+=¢ENDE  +                                                       "
OK=WL(Z)
Z=")INIT                                                               "
OK=WL(Z)
Z="  .ZVARS  = '(SQLCODE                                     +         "
OK=WL(Z)
Z="              msg400                                      +         "
OK=WL(Z)
Z="             )'                                                     "
OK=WL(Z)
Z="  .HELP   = CECTHELP                                                "
OK=WL(Z)
Z="  .CURSOR = SQLCODE                                                 "
OK=WL(Z)
Z="  &ZCMD   = &Z                                                      "
OK=WL(Z)
Z=")REINIT                                                             "
OK=WL(Z)
Z="  REFRESH(*)                                                        "
OK=WL(Z)
