SKEL_SKDIAG:
Z=")DEFAULT  )#?ü<~>                                                   "
OK=WL(Z)
Z="//DIAGNOS EXEC PGM=DSNUTILB,                                        "
OK=WL(Z)
Z="//         REGION=8M,                                               "
OK=WL(Z)
Z="//         PARM=(#SSID,'DIAGNOS')                                   "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSPRINT  DD SYSOUT=*                                             "
OK=WL(Z)
Z="//*SYSPRINT DD  DCB=(RECFM=VM,LRECL=133),                           "
OK=WL(Z)
Z="//*             DISP=(NEW,CATLG,CATLG),                             "
OK=WL(Z)
Z="//*             DSORG=PS,                                           "
OK=WL(Z)
Z="//*             SPACE=(CYL,(5,5)),                                  "
OK=WL(Z)
Z="//*             DSN=#USERID..DSNAME.FOR.OUTPUT                      "
OK=WL(Z)
Z="//UTPRINT   DD SYSOUT=*                                             "
OK=WL(Z)
Z="//RNPRIN01  DD SYSOUT=*                                             "
OK=WL(Z)
Z="//STPRIN01  DD SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSIN     DD *                                                    "
OK=WL(Z)
Z=" DIAGNOSE                                                           "
OK=WL(Z)
Z=")IF #AUSWTYP = Y  THEN )DO                                          "
OK=WL(Z)
Z="     TYPE(#Type)                                                    "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")IF #Alldumps = Y  THEN )DO                                         "
OK=WL(Z)
Z="     ALLDUMPS (x'#AlldumpA')                                        "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")IF #NODumps  = Y  THEN )DO                                         "
OK=WL(Z)
Z="     NODUMPS (x'#NODumpA')                                          "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")IF #SYSUTIL = Y  THEN )DO                                          "
OK=WL(Z)
Z="     DISPLAY SYSUTIL                                                "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")IF #MEPL    = Y  THEN )DO                                          "
OK=WL(Z)
Z="     DISPLAY MEPL                                                   "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")IF #AVAILAB = Y  THEN )DO                                          "
OK=WL(Z)
Z="     DISPLAY AVAILABLE                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")IF #OBD     = Y  THEN )DO                                          "
OK=WL(Z)
Z="     DISPLAY OBD #OBDOBJ #OBDAUSW                                   "
OK=WL(Z)
Z=")IF #OBDCLON = Y  THEN )DO                                          "
OK=WL(Z)
Z="                  CLONE                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")IF #DBET    = Y  THEN )DO                                          "
OK=WL(Z)
Z="     DISPLAY DBET                                                   "
OK=WL(Z)
Z=")IF #DBETO    = DB THEN )DO                                         "
OK=WL(Z)
Z="             DATABASE #DBETOBJ                                      "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")IF #DBETO    = TS THEN )DO                                         "
OK=WL(Z)
Z="             TABLESPACE #DBETOBJ                                    "
OK=WL(Z)
Z=")IF #DBETCLON = Y  THEN )DO                                         "
OK=WL(Z)
Z="                  CLONE                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")IF #DBETO    = IX THEN )DO                                         "
OK=WL(Z)
Z="             INDEX    #DBETOBJ                                      "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=" DIAGNOSE END                                                       "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="// IF (DIAGNOS.RC GT 4 OR DIAGNOS.ABEND = TRUE) THEN                "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSTSPRT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSPRINT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSUDUMP DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSIN    DD  DUMMY                                                "
OK=WL(Z)
Z="//SYSTSIN  DD  *                                                    "
OK=WL(Z)
Z=" DSN SYSTEM(#SSID.)                                                 "
OK=WL(Z)
Z="     -DIS  UTILITY(DIAGNOS)                                         "
OK=WL(Z)
Z="     -TERM UTILITY(DIAGNOS)                                         "
OK=WL(Z)
Z="  END                                                               "
OK=WL(Z)
Z="// ENDIF                                                            "
OK=WL(Z)

RETURN

