PAN_PAUSW200:
Z=")ATTR                                                               "
OK=WL(Z)
Z=" § AREA(DYNAMIC)   SCROLL(ON) EXTEND(ON) DATAMOD(05)                "
OK=WL(Z)
Z=" @ AREA(DYNAMIC)   SCROLL(ON) EXTEND(ON) DATAMOD(05)                "
OK=WL(Z)
Z=" 01 TYPE(DATAOUT)  COLOR(RED)                                       "
OK=WL(Z)
Z=" 02 TYPE(DATAOUT)  COLOR(BLUE)                                      "
OK=WL(Z)
Z=" 03 TYPE(DATAOUT)  COLOR(GREEN)                                     "
OK=WL(Z)
Z=" 04 TYPE(DATAOUT)  COLOR(WHITE)                                     "
OK=WL(Z)
Z=" 05 TYPE(DATAIN)   PAS(OFF)      CAPS(ON)                           "
OK=WL(Z)
Z=" 06 TYPE(DATAIN)   PAS(ON)       CAPS(ON) HILITE(USCORE)            "
OK=WL(Z)
Z=" 07 TYPE(DATAIN)   COLOR(RED)    CAPS(ON) JUST(LEFT)                "
OK=WL(Z)
Z="  + TYPE(TEXT) INTENS(LOW) SKIP(ON)                                 "
OK=WL(Z)
Z="  $ TYPE(TEXT) INTENS(HIGH) SKIP(ON) color(pink)                    "
OK=WL(Z)
Z="  * TYPE(OUTPUT) INTENS(LOW) SKIP(ON)                               "
OK=WL(Z)
Z="  | TYPE(OUTPUT) INTENS(HIGH) SKIP(ON)                              "
OK=WL(Z)
Z=")BODY EXPAND(~~)                                                    "
OK=WL(Z)
