Z=")IF #auswkz = DB THEN )DO                                           "
OK=WL(Z)
Z="          INCLUDE TABLESPACES DATABASE #DB                          "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z=")IF #USEDOT  = Y THEN )DO                                           "
OK=WL(Z)
Z=")DOT #TSLISTE                                                       "
OK=WL(Z)
Z="          INCLUDE TABLESPACE #DBNAME..#TSNAME.                      "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z="          INCLUDE TABLESPACE #DB..#TS.                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="//SYSIN DD *                                                        "
OK=WL(Z)
Z="--OPTIONS PREVIEW                                                   "
OK=WL(Z)
Z="--OPTIONS EVENT(ITEMERROR,SKIP)                                     "
OK=WL(Z)
Z="  QUIESCE LIST LQUIESCE                                             "
OK=WL(Z)
Z=")IF #DBVers GT 10  THEN )DO                                         "
OK=WL(Z)
Z="--        WRITE YES/NO                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="// IF (QUIESCE.RC GT 4 OR QUIESCE.ABEND = TRUE) THEN                "
OK=WL(Z)
