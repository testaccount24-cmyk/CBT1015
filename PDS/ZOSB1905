Z=")DEFAULT  )#?ü<~>                                               "
OK=WL(Z)
Z="//STOPDB   EXEC PGM=IKJEFT01                                    "
OK=WL(Z)
Z=")IM SKSTEPLI                                                    "
OK=WL(Z)
Z="//SYSTSPRT DD SYSOUT=*                                          "
OK=WL(Z)
Z="//SYSPRINT DD SYSOUT=*                                          "
OK=WL(Z)
Z="//SYSTSIN  DD *                                                 "
OK=WL(Z)
Z="DSN SYSTEM(#SSID.)                                              "
OK=WL(Z)
Z="  -STOP DATABASE(#DB.) SPACENAM(#TS.)                           "
OK=WL(Z)
Z=" END                                                            "
OK=WL(Z)
Z="/*                                                              "
OK=WL(Z)
Z=")SEL #VSAMCL  EQ Y                                              "
OK=WL(Z)
Z="//ADDVOL   EXEC PGM=IDCAMS                                      "
OK=WL(Z)
Z="//SYSPRINT DD  SYSOUT=*                                         "
OK=WL(Z)
Z="//SYSIN    DD  *                                                "
OK=WL(Z)
Z="           ALTER #VOLDS -                                       "
OK=WL(Z)
Z="           ADDVOLUMES(* * * * * * * * * * * * * * * * * * * *)  "
OK=WL(Z)
Z="/*                                                              "
OK=WL(Z)
Z="//DEFCLU EXEC PGM=IDCAMS                                        "
OK=WL(Z)
Z="//SYSPRINT DD SYSOUT=*                                          "
OK=WL(Z)
Z="//SYSIN DD *                                                    "
OK=WL(Z)
Z=")DOT #VSAMDS EQ Y                                               "
OK=WL(Z)
Z=" DEFINE CLUSTER ( -                                             "
OK=WL(Z)
Z=" NAME  ('#CDSN') -                                              "
OK=WL(Z)
Z=" MODEL ('#MODELDS)') -                                          "
OK=WL(Z)
Z=" ) -                                                            "
OK=WL(Z)
Z=" DATA  ( -                                                      "
OK=WL(Z)
Z=" NAME  ('#DDSN')      -                                         "
OK=WL(Z)
Z=" MODEL ('#VOLDS)')     -                                        "
OK=WL(Z)
Z=" )                                                              "
OK=WL(Z)
Z=")BLANK                                                          "
OK=WL(Z)
Z=")ENDDOT                                                         "
OK=WL(Z)
Z="/*                                                              "
OK=WL(Z)
Z=")ENDSEL                                                         "
OK=WL(Z)
Z="//DSN1CPY  EXEC PGM=DSN1COPY,                                   "
OK=WL(Z)
Z=")IF #TSPART GT 0 THEN )DO                                       "
OK=WL(Z)
Z="//      PARM='#CPTYPE,OBIDXLAT,RESET,NUMPARTS(#TSPART)#TLOB'    "
OK=WL(Z)
Z=")ENDDO                                                          "
OK=WL(Z)
Z=")ELSE )DO                                                       "
OK=WL(Z)
Z="//      PARM='#CPTYPE,OBIDXLAT,RESET#TLOB'                      "
OK=WL(Z)
Z=")ENDDO                                                          "
OK=WL(Z)
Z=")IM SKSTEPLI                                                    "
OK=WL(Z)
Z="//SYSUT1   DD DISP=SHR,DSN=#FDSN                                "
OK=WL(Z)
Z="//SYSUT2   DD DISP=SHR,DSN=#TDSN                                "
OK=WL(Z)
Z="//SYSPRINT DD SYSOUT=*                                          "
OK=WL(Z)
Z="//* FROM,TO                                                     "
OK=WL(Z)
Z="//SYSXLAT  DD *                                                 "
OK=WL(Z)
Z="#FDBID.,#TDBID.  DB=#DB.                                        "
OK=WL(Z)
Z="#FTSID.,#TTSID.  TS=#TS.                                        "
OK=WL(Z)
Z="#FTBID.,#TTBID   TB=#FTB/#TTB                                   "
OK=WL(Z)
Z="/*                                                              "
OK=WL(Z)
Z="//STARTDB  EXEC PGM=IKJEFT01                                    "
OK=WL(Z)
Z=")IM SKSTEPLI                                                    "
OK=WL(Z)
Z="//SYSTSPRT DD SYSOUT=*                                          "
OK=WL(Z)
Z="//SYSPRINT DD SYSOUT=*                                          "
OK=WL(Z)
Z="//SYSTSIN  DD *                                                 "
OK=WL(Z)
Z="DSN SYSTEM(#SSID.)                                              "
OK=WL(Z)
Z="  -START DATABASE(#DB.) SPACENAM(#TS.)                          "
OK=WL(Z)
Z=" END                                                            "
OK=WL(Z)
Z="/*                                                              "
OK=WL(Z)
Z="//REBUILD EXEC PGM=DSNUTILB,                                    "
OK=WL(Z)
Z="//*        REGION=8M,                                           "
OK=WL(Z)
Z="//         PARM=(#SSID,'REBUILD.#TS')                           "
OK=WL(Z)
Z=")IM SKSTEPLI                                                    "
OK=WL(Z)
Z="//SYSPRINT  DD SYSOUT=*                                         "
OK=WL(Z)
Z="//UTPRINT   DD SYSOUT=*                                         "
OK=WL(Z)
Z="//SYSIN    DD *                                                 "
OK=WL(Z)
Z="  REBUILD INDEX(ALL) TABLESPACE #DB..#TS.                       "
OK=WL(Z)
Z="/*                                                              "
OK=WL(Z)
Z=")SEL #LOB NE Y                                                  "
OK=WL(Z)
Z="//REPCAT  EXEC PGM=DSNUTILB,                                    "
OK=WL(Z)
Z="//*        REGION=8M,                                           "
OK=WL(Z)
Z="//         PARM=(#SSID,'REPCAT.#TS')                            "
OK=WL(Z)
Z=")IM SKSTEPLI                                                    "
OK=WL(Z)
Z="//SYSPRINT  DD SYSOUT=*                                         "
OK=WL(Z)
Z="//UTPRINT   DD SYSOUT=*                                         "
OK=WL(Z)
Z="//SYSIN    DD *                                                 "
OK=WL(Z)
Z="  REPAIR CATALOG TABLESPACE #DB..#TS.                           "
OK=WL(Z)
Z="/*                                                              "
OK=WL(Z)
Z=")ENDSEL                                                         "
OK=WL(Z)
Z="// IF (REBUILD.RC GT 4 OR REBUILD.ABEND = TRUE) THEN            "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                         "
OK=WL(Z)
Z=")IM SKSTEPLI                                                    "
OK=WL(Z)
Z="//SYSTSPRT DD  SYSOUT=*                                         "
OK=WL(Z)
