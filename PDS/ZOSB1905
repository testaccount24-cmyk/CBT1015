SKEL_SKBPG1:
Z=")DEFAULT  )#?ü<~>                                                   "
OK=WL(Z)
Z="//BINDPG EXEC PGM=IKJEFT01,DYNAMNBR=20                            "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//DBRMLIB  DD DISP=SHR,DSN=#DBRM1                                   "
OK=WL(Z)
Z="//SYSTERM  DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSPRINT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSTSPRT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSTSIN  DD *                                                     "
OK=WL(Z)
Z="DSN SYSTEM(#SSID.)                                                  "
OK=WL(Z)
Z=")SEL #VER EQ #BLANK                                                 "
OK=WL(Z)
Z="    BIND PACKAGE (#CR..#NM)                                       "
OK=WL(Z)
Z=")ENDSEL                                                           "
OK=WL(Z)
Z=")SEL #VER NE #BLANK                                               "
OK=WL(Z)
Z="    BIND PACKAGE (#CR)               -                            "
OK=WL(Z)
Z="                 MEM(#NM)            -                            "
OK=WL(Z)
Z="                 OWNER(#OWNER)       -                            "
OK=WL(Z)
Z="                 QUALIFIER(#QUALIF) -                             "
OK=WL(Z)
Z="                 SQLERROR(#SQLERR)  -                             "
OK=WL(Z)
Z="                 VALID(#VALOPT)     -                             "
OK=WL(Z)
Z="                 ISOL(#ISOLAT) -                                  "
OK=WL(Z)
Z="                 EXPL(#EXPL)        -                             "
OK=WL(Z)
Z="                 DEGREE(#DEGREE)    -                             "
OK=WL(Z)
