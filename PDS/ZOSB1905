PAN_PALTERTS:
Z=")ATTR                                                               "
OK=WL(Z)
Z="  | type(input) intens(high)             caps(on ) pad(' ')         "
OK=WL(Z)
Z="  @ type(output) intens(low ) just(left) caps(off) padc('')         "
OK=WL(Z)
Z="  ? type(output)intens(high) just(left)  caps(off) padc('')         "
OK=WL(Z)
Z=")BODY  EXPAND(//)                                                   "
OK=WL(Z)
Z="%-/-/- Alter   ?auswtext                                            "
OK=WL(Z)
Z="%Command ===>_zcmd                              / / %Scroll ===>_AMT"
OK=WL(Z)
Z="+ +                                                                 "
OK=WL(Z)
Z="+Name                +Old Value           +New Value       +        "
OK=WL(Z)
Z="+--------------      +------------------- +-----------     +        "
OK=WL(Z)
Z=")MODEL                                                              "
OK=WL(Z)
Z="@Name               @OVal                |NVal                     ",
  "           +"
OK=WL(Z)
Z=")INIT                                                               "
OK=WL(Z)
Z=")PROC                                                               "
OK=WL(Z)
Z="&PF = .PFKEY                                                        "
OK=WL(Z)
Z="&PANELRC = 0                                                        "
OK=WL(Z)
Z="IF (.RESP = END,RETURN)                                             "
OK=WL(Z)
Z="    .RESP = ENTER                                                   "
OK=WL(Z)
Z="     &PANELRC = 8                                                   "
OK=WL(Z)
Z="IF (&ZCMD = CAN,CANCEL)                                             "
OK=WL(Z)
Z="   &PANELRC = 9                                                     "
OK=WL(Z)
Z=")End                                                                "
OK=WL(Z)

RETURN


PAN_PCOLCOMP:
Z=")ATTR                                                               "
OK=WL(Z)
Z=" + TYPE(TEXT)   INTENS(HIGH)                                        "
OK=WL(Z)
Z=" # TYPE(TEXT)   INTENS(LOW)                                         "
OK=WL(Z)
Z=" _ TYPE(INPUT)  INTENS(LOW) PAD(_)                                  "
OK=WL(Z)
Z=" | TYPE(INPUT)  INTENS(LOW)                                         "
OK=WL(Z)
Z=" % TYPE(OUTPUT) INTENS(HIGH)                                        "
OK=WL(Z)
Z=")BODY                                                               "
OK=WL(Z)
Z="+----------  Vergleichen zweier Tabellen auf Column ¢nderungen  ----"
OK=WL(Z)
Z="+Command ===>_Z                                              +" ||,
  "Scroll ===>_z    +"
OK=WL(Z)
Z="+Location From/To : _PFLOC            +_PTLOC             +         "
OK=WL(Z)
Z="+Schema/Name FROM : _PFTBCR      +_PFTBNA                           "
OK=WL(Z)
Z="+Schema/Name   TO : _PTTBCR      +_PTTBNA                           "
OK=WL(Z)
Z="#-------------------------------------------------------------------"
OK=WL(Z)
Z="+SE  COLNAME              NO   TYPE         LEN  SCA  N D DEF.Value "
OK=WL(Z)
Z="#-------------------------------------------------------------------"
OK=WL(Z)
Z=")MODEL                                                              "
OK=WL(Z)
Z="|Z  |Z                    |Z  |Z           |Z   |Z   |Z|Z|Z    " || ,
  "               +"
OK=WL(Z)
Z="    |Z                    |Z  |Z           |Z   |Z   |Z|Z|Z    " || ,
  "               +"
OK=WL(Z)
Z=")INIT                                                               "
OK=WL(Z)
Z=".ZVARS = '(COM,ZSCED,                                            +  "
OK=WL(Z)
Z="           COLSEL,                                               +  "
OK=WL(Z)
Z="           FNAME,FCOLNO,FCOLTYPE,FLENGTH,FSCALE,FNULLS,FDEFAULT, +  "
OK=WL(Z)
Z="           FDEFAULV,                                             +  "
OK=WL(Z)
Z="           TNAME,TCOLNO,TCOLTYPE,TLENGTH,TSCALE,TNULLS,TDEFAULT, +  "
OK=WL(Z)
Z="           TDEFAULV                                              +  "
OK=WL(Z)
Z="         )'                                                         "
OK=WL(Z)
Z=".ZVARS = '(ZSCED)'                                                  "
OK=WL(Z)
Z="VGET (ZSCED) PROFILE                                                "
OK=WL(Z)
Z="IF (&ZSCED = ' ') &ZSCED = 'PAGE'  /* Blank with page    */         "
OK=WL(Z)
Z=")REINIT                                                             "
OK=WL(Z)
Z="  REFRESH(*)                                                        "
OK=WL(Z)
Z="  .CURSOR = COM                                                     "
OK=WL(Z)
Z=")PROC                                                               "
OK=WL(Z)
Z="  &PFTASTE  = .PFKEY                                                "
OK=WL(Z)
Z="  &PFINHALT = TRUNC(&PFTASTE,2)                                     "
OK=WL(Z)
Z="  &PFINHALT = .TRAIL                                                "
OK=WL(Z)
Z="  &PFINHALT = PFK(&PFINHALT)                                        "
OK=WL(Z)
Z="  &PANELRC = 0                                                      "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="  IF (.RESP = END,RETURN)                                           "
OK=WL(Z)
Z="    &PANELRC = 4                                                    "
OK=WL(Z)
Z="    .RESP    = ENTER                                                "
OK=WL(Z)
Z="  IF (&PFINHALT = RETURN)                                           "
OK=WL(Z)
Z="    &PANELRC = 8                                                    "
OK=WL(Z)
Z="    .RESP    = ENTER                                                "
OK=WL(Z)
Z="  IF (&COM  = CAN,CANCEL)                                           "
OK=WL(Z)
Z="    &PANELRC = 9                                                    "
OK=WL(Z)
Z="    .RESP    = ENTER                                                "
OK=WL(Z)
Z="  &VZCMD1 = TRUNC(&COM,' ')                                         "
OK=WL(Z)
Z="  &VZCMD2 = .TRAIL                                                  "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")END                                                                "
OK=WL(Z)

RETURN

