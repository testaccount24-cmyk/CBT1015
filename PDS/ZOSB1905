SKEL_SKREPCAT:
Z=")DEFAULT  )#?ü<~>                                               "
OK=WL(Z)
Z="//REPCAT  EXEC PGM=DSNUTILB,                                    "
OK=WL(Z)
Z="//         REGION=8M,                                           "
OK=WL(Z)
Z="//         PARM=(#SSID,'REPCAT.#TS')                            "
OK=WL(Z)
Z=")IM SKSTEPLI                                                    "
OK=WL(Z)
Z="//SYSPRINT  DD SYSOUT=*                                         "
OK=WL(Z)
Z="//UTPRINT   DD SYSOUT=*                                         "
OK=WL(Z)
Z="//*DFSPARM DD *                                                 "
OK=WL(Z)
Z="//*     OPTION FILSZ=E99999999,DYNALLOC=(#SYSDA,9)              "
OK=WL(Z)
Z="//SYSIN DD *                                                    "
OK=WL(Z)
Z="--OPTIONS PREVIEW                                               "
OK=WL(Z)
Z="--OPTIONS EVENT(ITEMERROR,SKIP)                                 "
OK=WL(Z)
Z=")IF #USEDOT  = Y THEN )DO                                       "
OK=WL(Z)
Z=")DOT #TSLISTE                                                   "
OK=WL(Z)
Z="  REPAIR CATALOG TABLESPACE #DBNAME..#TSNAME.                   "
OK=WL(Z)
Z=")ENDDOT                                                         "
OK=WL(Z)
Z=")ENDDO                                                          "
OK=WL(Z)
Z=")ELSE )DO                                                       "
OK=WL(Z)
Z="  REPAIR CATALOG TABLESPACE #DB..#TS.                           "
OK=WL(Z)
Z=")ENDDO                                                          "
OK=WL(Z)
Z="                                                                "
OK=WL(Z)
Z="/*                                                              "
OK=WL(Z)
Z="// IF (REPCAT.RC GT 4 OR REPCAT.ABEND = TRUE) THEN              "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                         "
OK=WL(Z)
Z=")IM SKSTEPLI                                                    "
OK=WL(Z)
Z="//SYSTSPRT DD  SYSOUT=*                                         "
OK=WL(Z)
Z="//SYSPRINT DD  SYSOUT=*                                         "
OK=WL(Z)
Z="//SYSUDUMP DD  SYSOUT=*                                         "
OK=WL(Z)
Z="//SYSIN    DD  DUMMY                                            "
OK=WL(Z)
Z="//SYSTSIN  DD  *                                                "
OK=WL(Z)
Z="  DSN SYSTEM(#SSID.)                                            "
OK=WL(Z)
Z="  -DIS  UTILITY(REPCAT.#TS)                                     "
OK=WL(Z)
Z="  -TERM UTILITY(REPCAT.#TS)                                     "
OK=WL(Z)
Z="  END                                                           "
OK=WL(Z)
Z="// ENDIF                                                        "
OK=WL(Z)

RETURN

