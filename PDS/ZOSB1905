OK=WL(Z)
Z="--         DSN #MDICTAPP                                            "
OK=WL(Z)
Z="           DSN #MDICTAPE                                            "
OK=WL(Z)
Z="           UNIT #TAPE                                               "
OK=WL(Z)
Z="           DISP (MOD,CATLG,CATLG)                                   "
OK=WL(Z)
Z="           DATACLAS #RACDATAC                                       "
OK=WL(Z)
Z="           VOLCNT   255                                             "
OK=WL(Z)
Z="           RETPD 30                                                 "
OK=WL(Z)
Z="           STACK(YES)                                               "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="--TEMPLATE COPYDISK                                                 "
OK=WL(Z)
Z="--         DSN #MDICDISP                                            "
OK=WL(Z)
Z="--         DSN #MDICDISK                                            "
OK=WL(Z)
Z="--         DISP (NEW,CATLG,CATLG)                                   "
OK=WL(Z)
Z="--         UNIT #SYSDA                                              "
OK=WL(Z)
Z="--         UNCNT    59                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="//SYSIN DD *                                                        "
OK=WL(Z)
Z="--OPTIONS PREVIEW                                                   "
OK=WL(Z)
Z=")IF #USEDOT  = Y THEN )DO                                           "
OK=WL(Z)
Z=")DOT #TSLISTE                                                       "
OK=WL(Z)
Z="COPYTOCOPY TABLESPACE #DBNAME..#TSNAME.                             "
OK=WL(Z)
Z=")SEL #DSNAME NE #BLANK                                              "
OK=WL(Z)
Z="      FROMCOPY #DSNAME.                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #DSNAME EQ #BLANK                                              "
OK=WL(Z)
Z="      FROMLASTFULLCOPY                                              "
OK=WL(Z)
Z="--    FROMLASTCOPY                                                  "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="      RECOVERYDDN(COPYTAPE)                                         "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z="COPYTOCOPY TABLESPACE #DB..#TS.                                     "
OK=WL(Z)
Z=")SEL #ICDSN NE #BLANK                                               "
OK=WL(Z)
Z="      FROMCOPY #ICDSN                                               "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #ICDSN EQ #BLANK                                               "
OK=WL(Z)
Z="      FROMLASTFULLCOPY                                              "
OK=WL(Z)
Z="--    FROMLASTCOPY                                                  "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="      RECOVERYDDN(COPYTAPE)                                         "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="// IF (CPY2CPY.RC GT 4 OR CPY2CPY.ABEND = TRUE) THEN                "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
OK=WL(Z)
