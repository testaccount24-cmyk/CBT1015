PGLI PG         AUSWAHLOK = 'Y'

ENV D           OBJECT1    = ENVID    || ".*"
ENV D           DB2_LIST_OBJ = SYSIBMS".SYSENVIRONMENT"
ENV D           AUSWAHLOK = 'Y'

CODIS D         OBJECT1    = STRIP(TBOWNER) || "." || STRIP(TBNAME)
CODIS D         OBJECT2    = NAME || "." || FREQUENCY
CODIS D         OBJECT3    = STATSTIME
CODIS D         DB2_LIST_OBJ = SYSIBMS".SYSCOLDIST"
CODIS D         AUSWAHLOK = 'Y'

CODIS CO        OBJECT1    = "*." || NAME
CODIS CO        OBJECT2    = STRIP(TBOWNER) || "." || STRIP(TBNAME)
CODIS CO        AUSWAHLOK = 'Y'

CODIS TB        OBJECT1    = STRIP(TBOWNER) || "." || STRIP(TBNAME)
CODIS TB        AUSWAHLOK  = 'Y'

COSTATS D       OBJECT1    = STRIP(TBOWNER) || "." || STRIP(TBNAME)
COSTATS D       OBJECT2    = NAME || "." || PARTITION
COSTATS D       DB2_LIST_OBJ = SYSIBMS".SYSCOLSTATS"
COSTATS D       AUSWAHLOK = 'Y'

COSTATS CO      OBJECT1    = "*." || NAME
COSTATS CO      OBJECT2    = STRIP(TBOWNER) || "." || STRIP(TBNAME)
COSTATS CO      AUSWAHLOK = 'Y'

COSTATS TB      OBJECT1    = STRIP(TBOWNER) || "." || STRIP(TBNAME)
COSTATS TB      AUSWAHLOK  = 'Y'

LVU   D         OBJECT1    = STRIP(EFFECTIVE_TIME)
LVU   D         DB2_LIST_OBJ = SYSIBMS".SYSLEVELUPDATES"
LVU   D         AUSWAHLOK = 'Y'
