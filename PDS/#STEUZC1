RT PG                  OBJECT2 = ""
RT PG              END
RT PG             WHEN STRIP(EXTERNAL_NAME) = "" THEN DO
RT PG                  OBJECT1 = STRIP(SCHEMA) || '.' || STRIP(NAME)
RT PG
RT PG                  OBJECT2 = ""
RT PG              END
RT PG           OTHERWISE DO
RT PG                  OBJECT1 = STRIP(COLLID)||'.'
RT PG                  OBJECT1 = OBJECT1 ||  STRIP(EXTERNAL_NAME)
RT PG                  OBJECT2 = ""
RT PG              END
RT PG            END
RT PG           AUSWAHLOK = 'Y'

