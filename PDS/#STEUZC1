TCOATTR TCO     AUSWAHLOK = 'Y'

THD DIS         OBJECT1    = STRIP(TOKEN)
THD DIS         CALL AUFRUF_ZOSC1915
THD DIS         AUSWAHLOK = 'N'


THD RECOVER     IF TYPE    = 3 THEN DO
THD RECOVER        IPADDR = SUBSTR(COORDINATOR, 3)
THD RECOVER        PARSE VALUE IPADDR WITH IP ".." PORT
THD RECOVER        TOKEN  = PORT
THD RECOVER      END
THD RECOVER     OBJECT1    = STRIP(TOKEN) || "." || STRIP(LUWID)
THD RECOVER     OBJECT2    = STRIP(NAME)  || "." || STRIP(ID)
THD RECOVER     OBJECT3    = STRIP(LOCATION_WS) || "." || LUNAME
THD RECOVER     IF TYPE    = 3 THEN IPADDR = SUBSTR(COORDINATOR, 3)
THD RECOVER     IF TYPE    = 3 THEN PARSE VALUE IPADDR WITH IP ".." PORT
THD RECOVER     OBJECT4    = IPADDR
THD RECOVER     IF TYPE    = 3 THEN OBJECT5 = "*.INDOUBT"
THD RECOVER     IF TYPE    = 4 THEN OBJECT5 = "*.POSTPONED"
THD RECOVER     SELECT
THD RECOVER       WHEN WORD(TYPE, 1) = '3' THEN   CALL AUFRUF_ZOSC1915
THD RECOVER       WHEN WORD(TYPE, 1) = '4' THEN   CALL AUFRUF_ZOSC1915
THD RECOVER       OTHERWISE NOP
THD RECOVER      END
THD RECOVER     AUSWAHLOK = 'N'


THD RESET       OBJECT1    = STRIP(TOKEN) || "." || STRIP(LUWID)
THD RESET       OBJECT2    = STRIP(NAME)  || "." || STRIP(ID)
THD RESET       OBJECT3    = STRIP(LOCATION_WS) || "." || LUNAME
THD RESET       IF TYPE    = 3 THEN IPADDR = SUBSTR(COORDINATOR, 3)
THD RESET       OBJECT4    = IPADDR
THD RESET       OBJECT5    = ""
THD RESET       IF TYPE    = 3 THEN OBJECT5 = "*.INDOUBT"
THD RESET       IF TYPE    = 4 THEN OBJECT5 = "*.POSTPONED"
THD RESET       SELECT
THD RESET         WHEN WORD(TYPE, 1) = '3' THEN   CALL AUFRUF_ZOSC1915
THD RESET         WHEN WORD(TYPE, 1) = '4' THEN   CALL AUFRUF_ZOSC1915
THD RESET         OTHERWISE NOP
THD RESET        END
THD RESET       AUSWAHLOK = 'N'


THD DROPIP      IF TYPE = 3 THEN IPADDR = SUBSTR(COORDINATOR, 3)
THD DROPIP      OBJECT1    = IPADDR
THD DROPIP      IF LENGTH(STRIP(IPADDR)) > 0 THEN CALL AUFRUF_ZOSC1915
THD DROPIP      AUSWAHLOK = 'N'

THD CAN         OBJECT1    = STRIP(TOKEN)
THD CAN         CALL AUFRUF_ZOSC1915
THD CAN         AUSWAHLOK = 'N'

THD D           OBJECT1    = STRIP(NAME) || "." || STRIP(TOKEN)
THD D           DB2_LIST_OBJ = SYSIBMS".ADMIN_COMMAND_DB2_THD"
THD D           AUSWAHLOK = 'Y'

