STATS D         SELECT
STATS D           WHEN STATS_TYPE = 'IX' THEN DO
STATS D                OBJECT1    = OWNER  || "." || NAME
STATS D                OBJECT2    = PARTITION"." || "*"
STATS D                DB2_LIST_OBJ = SYSIBMS".SYSINDEXSTATS"
STATS D            END
STATS D           WHEN STATS_TYPE = 'TS' THEN DO
STATS D                OBJECT1    = OWNER  || "." || NAME
STATS D                OBJECT2    = PARTITION"." || "*"
STATS D                DB2_LIST_OBJ = SYSIBMS".SYSTABSTATS"
STATS D            END
STATS D           OTHERWISE  DO
STATS D                OBJECT1    = DBNAME || "." || NAME
STATS D                DB2_LIST_OBJ = SYSIBMS".SYSLOBSTATS"
STATS D            END
STATS D          END
STATS D         AUSWAHLOK = 'Y'

