SKEL_SKRECOV1:
Z=")DEFAULT  )#?ü<~>                                                   "
OK=WL(Z)
Z="//RECOVER EXEC PGM=DSNUTILB,                                        "
OK=WL(Z)
Z="//*        REGION=8M,                                               "
OK=WL(Z)
Z="//         PARM=(#SSID,'RECOVER.#TS')                               "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSPRINT  DD SYSOUT=*                                             "
OK=WL(Z)
Z="//UTPRINT   DD SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSIN    DD *                                                     "
OK=WL(Z)
Z="--OPTIONS PREVIEW                                                   "
OK=WL(Z)
Z="--OPTIONS EVENT(ITEMERROR,SKIP)                                     "
OK=WL(Z)
Z=")SEL #WHAT    = LC                                                  "
OK=WL(Z)
Z=")SETF Rectodo = #STR(TOLASTCOPY)                                    "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #WHAT    = FC                                                  "
OK=WL(Z)
Z=")SETF Rectodo = #STR(TOLASTFULLCOPY)                                "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #WHAT    = LS                                                  "
OK=WL(Z)
Z=")SETF Rectodo = #STR(LOCALSITE CURRENTCOPYONLY)                     "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #WHAT    = IC                                                  "
OK=WL(Z)
Z=")SETF Rectodo = #STR(TOCOPY #OBJECT3.)                              "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #WHAT    = RBA                                                 "
OK=WL(Z)
Z=")SETF Rectodo = #STR(TORBA X'#OBJECT2.')                            "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #WHAT    = RBAB                                                "
OK=WL(Z)
Z=")SETF Rectodo = #STR(TORBA X'#OBJECT2.')                            "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #WHAT    = LP                                                  "
OK=WL(Z)
Z=")SETF Rectodo = #STR(TOLOGPOINT X'#OBJECT2.')                       "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #WHAT    = RB                                                  "
OK=WL(Z)
Z=")SETF Rectodo = #STR(RESTOREBEFORE X'#OBJECT2.')                    "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")IF #USEDOT  = Y THEN )DO                                           "
OK=WL(Z)
Z=")DOT #TSLISTE                                                       "
OK=WL(Z)
Z="  RECOVER  TABLESPACE #DBNAME..#TSNAME.                             "
OK=WL(Z)
Z="       #Rectodo                                                     "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z="  RECOVER  TABLESPACE #DB..#TS.                                     "
OK=WL(Z)
Z="       #Rectodo                                                     "
OK=WL(Z)
Z=")SEL #WHAT    = RBAB                                                "
OK=WL(Z)
Z="       BACKOUT YES                                                  "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")BLANK                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")IF #USEDOT  = Y THEN )DO                                           "
OK=WL(Z)
Z=")DOT #TSLISTE                                                       "
OK=WL(Z)
Z="  REBUILD INDEX(ALL) TABLESPACE #DBNAME..#TSNAME.                   "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z="  REBUILD INDEX(ALL) TABLESPACE #DB..#TS.                           "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="// IF (RECOVER.RC GT 4 OR RECOVER.ABEND = TRUE) THEN                "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSTSPRT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSPRINT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSUDUMP DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSIN    DD  DUMMY                                                "
OK=WL(Z)
Z="//SYSTSIN  DD  *                                                    "
OK=WL(Z)
Z="  DSN SYSTEM(#SSID.)                                                "
OK=WL(Z)
Z="  -DIS  UTILITY(RECOVER.#TS.)                                       "
OK=WL(Z)
Z="  -TERM UTILITY(RECOVER.#TS.)                                       "
OK=WL(Z)
Z="  END                                                               "
OK=WL(Z)
Z="// ENDIF                                                            "
OK=WL(Z)

