OK=WL(Z)
Z="  -DIS  UTILITY(REBUILD.INDEX)                                      "
OK=WL(Z)
Z="  -TERM UTILITY(REBUILD.INDEX)                                      "
OK=WL(Z)
Z="  END                                                               "
OK=WL(Z)
Z="// ENDIF                                                            "
