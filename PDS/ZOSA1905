OK=WL(Z)
Z="//SYSIN    DD  DUMMY                                                "
OK=WL(Z)
Z="//SYSTSIN  DD  *                                                    "
OK=WL(Z)
Z="  DSN SYSTEM(#SSID)                                                 "
OK=WL(Z)
Z="  -DIS  UTILITY(CHKDATA.#TS.)                                       "
OK=WL(Z)
Z="  -TERM UTILITY(CHKDATA.#TS.)                                       "
OK=WL(Z)
Z="  END                                                               "
OK=WL(Z)
Z="// ENDIF                                                            "
OK=WL(Z)

