OK=WL(Z)
Z="//*        REGION=8M,                                               "
OK=WL(Z)
Z="//         PARM=(#SSID,'RECOVER.#TS')                               "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSPRINT  DD SYSOUT=*                                             "
OK=WL(Z)
Z="//UTPRINT   DD SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSIN    DD *                                                     "
