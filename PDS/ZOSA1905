OK=WL(Z)
Z="//SYSUDUMP DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSIN    DD  DUMMY                                                "
OK=WL(Z)
Z="//SYSTSIN  DD  *                                                    "
OK=WL(Z)
Z="  DSN SYSTEM(#SSID.)                                                "
OK=WL(Z)
Z="  -DIS  UTILITY(MODHIS.#TS )                                        "
OK=WL(Z)
Z="  -TERM UTILITY(MODHIS.#TS )                                        "
OK=WL(Z)
