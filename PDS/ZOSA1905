OK=WL(Z)
Z=")IF #auswkz = DB THEN )DO                                           "
OK=WL(Z)
Z="          INCLUDE TABLESPACES DATABASE #DB                          "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z=")IF #USEDOT  = Y THEN )DO                                           "
OK=WL(Z)
Z=")DOT #TSLISTE                                                       "
OK=WL(Z)
Z="          INCLUDE TABLESPACE #DBNAME..#TSNAME.                      "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z="          INCLUDE TABLESPACE #DB..#TS.                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="//SYSIN DD *                                                        "
OK=WL(Z)
Z="  --OPTIONS PREVIEW                                                 "
OK=WL(Z)
Z="  --OPTIONS EVENT(ITEMERROR,SKIP)                                   "
OK=WL(Z)
Z="  MODIFY STATISTICS LIST MODSTATS                                   "
OK=WL(Z)
Z="--       DELETE ACCESSPATH DATE(#HISDAT)                            "
OK=WL(Z)
Z="         DELETE ALL                                                 "
OK=WL(Z)
Z="         AGE #DELAG                                                 "
OK=WL(Z)
