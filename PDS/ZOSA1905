OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="  CREATE TABLE #TBC..#TBN._CHK                                      "
OK=WL(Z)
Z="    LIKE       #TBC..#TBN.                                          "
OK=WL(Z)
Z="     ;                                                              "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="//CHKDATA EXEC PGM=DSNUTILB,                                        "
OK=WL(Z)
Z="//         REGION=8M,                                               "
OK=WL(Z)
Z="//         PARM=(#SSID,'CHKDATA.#TS.') ,RESTART(PHASE)              "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//*  //SYSUT1  DD DSN=#JHLQ...#DB..#TS..SYSUT1,                     "
OK=WL(Z)
Z="//*  //         DISP=(MOD,DELETE,DELETE),                           "
OK=WL(Z)
Z="//*  //         SPACE=(CYL,(1000,1000)),                            "
OK=WL(Z)
Z="//*  //         UNIT=(#WORK,50)                                     "
OK=WL(Z)
Z="//*  //SORTOUT DD DSN=#JHLQ...#DB..#TS..SORTOUT,                    "
OK=WL(Z)
Z="//*  //         DISP=(MOD,DELETE,DELETE),                           "
OK=WL(Z)
Z="//*  //         SPACE=(CYL,(1000,1000)),                            "
OK=WL(Z)
Z="//*  //         UNIT=(#WORK,50)                                     "
OK=WL(Z)
Z="//*  //SYSERR  DD DSN=#JHLQ...#DB..#TS..SYSERR,                     "
OK=WL(Z)
Z="//*  //         DISP=(MOD,DELETE,DELETE),                           "
OK=WL(Z)
Z="//*  //         SPACE=(CYL,(1000,1000)),                            "
OK=WL(Z)
Z="//*  //         UNIT=(#WORK,50)                                     "
OK=WL(Z)
Z="//DSSPRINT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSPRINT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSUDUMP DD SYSOUT=*                                              "
OK=WL(Z)
Z="//UTPRINT  DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSOUT   DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSLISTD DD *                                                     "
OK=WL(Z)
Z="//SYSTEMPL DD *                                                     "
OK=WL(Z)
Z="  TEMPLATE TSYSUT1                                                  "
OK=WL(Z)
Z="           UNIT     #WORK                                           "
OK=WL(Z)
Z="           UNCNT    59                                              "
OK=WL(Z)
Z="           DISP(MOD,DELETE,CATLG)                                   "
OK=WL(Z)
Z="           SPACE(800,800) CYL                                       "
OK=WL(Z)
Z="           DSN('#MHLQWRK..&DB..&TS..SYSUT1')                        "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="  TEMPLATE TSORTOUT                                                 "
OK=WL(Z)
Z="           UNIT     #WORK                                           "
OK=WL(Z)
Z="           UNCNT    59                                              "
OK=WL(Z)
Z="           DISP(MOD,DELETE,CATLG)                                   "
OK=WL(Z)
Z="           SPACE(800,800) CYL                                       "
OK=WL(Z)
Z="           DSN('#MHLQWRK..&DB..&TS..SORTOUT')                       "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="  TEMPLATE TSYSERR                                                  "
OK=WL(Z)
Z="           UNIT     #WORK                                           "
OK=WL(Z)
Z="           UNCNT    59                                              "
OK=WL(Z)
Z="           DISP(MOD,DELETE,CATLG)                                   "
OK=WL(Z)
Z="           SPACE(800,800) CYL                                       "
OK=WL(Z)
Z="           DSN('#MHLQWRK..&DB..&TS..SYSERR')                        "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="//SYSIN DD *                                                        "
OK=WL(Z)
Z=")IF #USEDOT  = Y THEN )DO                                           "
OK=WL(Z)
Z=")DOT #TSLISTE                                                       "
OK=WL(Z)
Z="  CHECK #TSTYPE TABLESPACE  #DBNAME..#TSNAME.                       "
OK=WL(Z)
Z="        SCOPE ALL                                                   "
OK=WL(Z)
Z="        SHRLEVEL REFERENCE                                          "
OK=WL(Z)
Z="        WORKDDN  (TSYSUT1,TSORTOUT)                                 "
OK=WL(Z)
Z="        ERRDDN   (TSYSERR)                                          "
OK=WL(Z)
Z="        SORTDEVT #WORK                                              "
OK=WL(Z)
Z="        SORTNUM  4                                                  "
OK=WL(Z)
Z="        DRAIN_WAIT 180 RETRY  10 RETRY_DELAY 10                     "
OK=WL(Z)
Z=")SEL #LTB EQ Y                                                      "
OK=WL(Z)
Z="        FOR EXCEPTION IN #TBC..#TBN. USE #TBC..#TBN._CHK            "
OK=WL(Z)
Z="        DELETE YES                                                  "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")BLANK                                                              "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z="  CHECK #TSTYPE TABLESPACE  #DB..#TS.                               "
OK=WL(Z)
Z="        SCOPE ALL                                                   "
OK=WL(Z)
Z="        SHRLEVEL REFERENCE                                          "
OK=WL(Z)
Z="        WORKDDN  (TSYSUT1,TSORTOUT)                                 "
OK=WL(Z)
Z="        ERRDDN   (TSYSERR)                                          "
OK=WL(Z)
Z="        SORTDEVT #WORK                                              "
OK=WL(Z)
Z="        SORTNUM  4                                                  "
OK=WL(Z)
Z="        DRAIN_WAIT 180 RETRY  10 RETRY_DELAY 10                     "
OK=WL(Z)
Z=")SEL #LTB EQ Y                                                      "
OK=WL(Z)
Z="        FOR EXCEPTION IN #TBC..#TBN. USE #TBC..#TBN._CHK            "
OK=WL(Z)
Z="        DELETE YES                                                  "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")BLANK                                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z=")SEL #LTB EQ Y                                                      "
OK=WL(Z)
Z="//DSNTEP4 EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSTSPRT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSPRINT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSUDUMP DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSTSIN  DD  *                                                    "
OK=WL(Z)
Z="  DSN SYSTEM(#SSID.)                                                "
OK=WL(Z)
Z="      RUN  PROGRAM(DSNTEP4) -                                       "
OK=WL(Z)
Z="           PLAN(DSNTEP4)    -                                       "
OK=WL(Z)
Z=")SEL #RUNLIB NE #BLANK                                              "
OK=WL(Z)
Z="           LIB('#RUNLIB')   -                                       "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="           PARMS('/ALIGN(MID)')                                     "
OK=WL(Z)
Z="   END                                                              "
OK=WL(Z)
Z="//SYSIN DD *                                                        "
OK=WL(Z)
Z="--##SET ROWS_FETCH -1                                               "
OK=WL(Z)
Z="--##SET ROWS_OUT  -1                                                "
OK=WL(Z)
Z="--##SET TOLWARN   YES                                               "
OK=WL(Z)
Z="--##SET TERMINATOR ;                                                "
OK=WL(Z)
Z="--##SET TOLARTHWRN YES                                              "
OK=WL(Z)
Z="--##SET MAXERRORS -1                                                "
OK=WL(Z)
Z="--##SET MULT_FETCH 1000                                             "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="  DROP   TABLE #TBC..#TBN._CHK                                      "
OK=WL(Z)
Z="     ;                                                              "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="// IF (CHKDATA.RC GT 4 OR CHKDATA.ABEND = TRUE) THEN                "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSTSPRT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSPRINT DD  SYSOUT=*                                             "
OK=WL(Z)
