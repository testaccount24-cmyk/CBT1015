OK=WL(Z)
Z="// IF (MODHIS.RC GT 4 OR MODHIS.ABEND = TRUE) THEN                  "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
