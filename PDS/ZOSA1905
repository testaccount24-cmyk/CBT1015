OK=WL(Z)
Z="//           REGION=8M,                                             "
OK=WL(Z)
Z="//           PARM=(#SSID,'UNLLOAD.#TS','')                          "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSPRINT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//UTPRINT  DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSIN   DD *                                                      "
OK=WL(Z)
Z="TEMPLATE UNLDSN                                                     "
OK=WL(Z)
Z="         DSN #MHLQUNL..&DB..&TS..UNLOAD                             "
OK=WL(Z)
Z="         UNIT #WORK                                                 "
OK=WL(Z)
Z="         DISP (NEW,CATLG,CATLG)                                     "
OK=WL(Z)
Z="         SPACE (50,20) CYL                                          "
OK=WL(Z)
Z="TEMPLATE PUNDSN                                                     "
OK=WL(Z)
Z="         DSN #MHLQUNL..&DB..&TS..PUNCH                              "
OK=WL(Z)
