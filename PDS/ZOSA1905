OK=WL(Z)
Z="         UNIT(#WORK)                                                "
OK=WL(Z)
Z="         DISP(NEW,DELETE,CATLG)                                     "
OK=WL(Z)
Z="         DSN(#MHLQRO..INDEX.SUT1)                                   "
OK=WL(Z)
Z="//SYSIN DD *                                                        "
OK=WL(Z)
Z="--OPTIONS PREVIEW                                                   "
OK=WL(Z)
Z="--OPTIONS EVENT(ITEMERROR,SKIP)                                     "
OK=WL(Z)
Z="  REORG INDEX      LIST ROLIST                                      "
OK=WL(Z)
Z="        SHRLEVEL CHANGE                                             "
OK=WL(Z)
Z="        SORTDEVT #WORK                                              "
OK=WL(Z)
Z="        WORKDDN (SUT1)                                              "
OK=WL(Z)
Z="        STATISTICS REPORT NO                                        "
OK=WL(Z)
Z="                   UPDATE ALL                                       "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="// IF (REORGIX.RC GT 4 OR REORGIX.ABEND = TRUE) THEN                "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
