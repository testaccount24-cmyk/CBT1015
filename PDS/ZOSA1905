OK=WL(Z)
Z=")IF #ICTYPE. = I THEN )DO                                           "
OK=WL(Z)
Z="       TOCOPY #DSNAME.                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z="       TORBA X'#STARTRBA.'                                          "
OK=WL(Z)
Z="       BACKOUT  NO                                                  "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")BLANK                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
