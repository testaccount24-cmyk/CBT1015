OK=WL(Z)
Z="       DISCARDDN    DISC                                            "
OK=WL(Z)
Z=")SEL #ICOPY  EQ Y                                                   "
OK=WL(Z)
Z="       COPYDDN      COPYDSN                                         "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #ONLINE EQ Y                                                   "
OK=WL(Z)
Z="       LOG          NO                                              "
OK=WL(Z)
Z="       DRAIN_WAIT   30                                              "
OK=WL(Z)
Z="       FASTSWITCH   YES                                             "
OK=WL(Z)
Z=")IF #USEMDB =  Y       THEN )DO                                     "
OK=WL(Z)
Z="       MAPPINGDATABASE #MAPDB                                       "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z="       MAPPINGTABLE #MAPTAB                                         "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="       MAXRO        50                                              "
OK=WL(Z)
Z="       LONGLOG      CONTINUE                                        "
OK=WL(Z)
Z="       TIMEOUT      TERM                                            "
OK=WL(Z)
Z=")SEL #STATS  EQ Y                                                   "
OK=WL(Z)
Z="       STATISTICS TABLE(ALL)                                        "
OK=WL(Z)
Z="                  INDEX(ALL KEYCARD FREQVAL NUMCOLS 1 COUNT 10)     "
OK=WL(Z)
Z="                  REPORT NO                                         "
OK=WL(Z)
Z="                  UPDATE ALL                                        "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="// IF (REORG.RC GT 4 OR REORG.ABEND = TRUE) THEN                    "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
OK=WL(Z)
