OK=WL(Z)
Z="//         PARM=(#SSID,'MODHIS.#TS.')                               "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSPRINT  DD SYSOUT=*                                             "
OK=WL(Z)
Z="//UTPRINT   DD SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSLISTD DD *                                                     "
OK=WL(Z)
