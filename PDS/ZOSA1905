OK=WL(Z)
Z="//STEPLIB  DD DISP=SHR,DSn=&LOAD1                                   "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//DBRMLIB  DD DISP=SHR,DSN=#DBRM1                                   "
OK=WL(Z)
Z="//SYSTERM  DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSPRINT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSTSPRT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSTSIN  DD *                                                     "
OK=WL(Z)
Z="DSN SYSTEM(#SSID.)                                                  "
OK=WL(Z)
Z=")SEL #VER EQ #BLANK                                                 "
OK=WL(Z)
Z="    REBIND PACKAGE (#CR..#NM)                                       "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=")SEL #VER NE #BLANK                                                 "
OK=WL(Z)
Z="    REBIND PACKAGE (#CR..#NM..(#VER.))                              "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z=" END                                                                "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)

RETURN

