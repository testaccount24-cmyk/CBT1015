SKEL_SKCHKIX1:
Z=")DEFAULT  )#?ü<~>                                                   "
OK=WL(Z)
Z="//CHECKIX EXEC PGM=DSNUTILB,                                        "
OK=WL(Z)
Z="//         REGION=8M,                                               "
OK=WL(Z)
Z="//         PARM=(#SSID,'CHECKIX.INDEX') ,RESTART(PHASE)             "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//ABNLIGNR DD DUMMY                                                 "
OK=WL(Z)
Z="//DSSPRINT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSPRINT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSUDUMP DD SYSOUT=*                                              "
OK=WL(Z)
Z="//UTPRINT  DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSOUT   DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSLISTD DD *                                                     "
OK=WL(Z)
Z="  LISTDEF CHECKIDX                                                  "
OK=WL(Z)
Z=")IF #USEDOT  = Y THEN )DO                                           "
OK=WL(Z)
Z=")DOT #TSLISTE                                                       "
OK=WL(Z)
Z="   INCLUDE INDEX      #CREATOR..#NAME.                              "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z="   INCLUDE INDEX      #IXCR..#IXNM                                  "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="//SYSTEMPL DD *                                                     "
OK=WL(Z)
Z="  TEMPLATE TSYSUT1                                                  "
OK=WL(Z)
Z="           UNIT     #WORK                                           "
OK=WL(Z)
