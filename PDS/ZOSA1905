OK=WL(Z)
Z="//UTPRINT   DD SYSOUT=*                                             "
OK=WL(Z)
Z="//RNPRIN01  DD SYSOUT=*                                             "
OK=WL(Z)
Z="//STPRIN01  DD SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSIN     DD *                                                    "
OK=WL(Z)
Z="--                                                                  "
OK=WL(Z)
Z="--OPTIONS PREVIEW                                                   "
OK=WL(Z)
Z="--OPTIONS EVENT(ITEMERROR,SKIP)                                     "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")REXX TB                                                            "
OK=WL(Z)
Z="   /*   TB='HUGO'  */                                               "
OK=WL(Z)
Z="   return                                                           "
OK=WL(Z)
Z=")ENDREXX                                                            "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")IF #USEDOT  = Y THEN )DO                                           "
OK=WL(Z)
Z=")DOT #TSLISTE                                                       "
OK=WL(Z)
Z=" RUNSTATS TABLESPACE #DBNAME..#TSNAME.                              "
OK=WL(Z)
Z="          SHRLEVEL CHANGE                                           "
OK=WL(Z)
Z=")IF #TYPE NE LOB THEN )DO                                           "
OK=WL(Z)
Z="          TABLE (#TB)                                               "
OK=WL(Z)
Z=")IF #USESTF = N THEN )DO                                            "
OK=WL(Z)
Z="          COLUMN (ALL)                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z=")DOT #COLLIST SCAN(TB,EQ)                                           "
OK=WL(Z)
Z="          #RSANW                                                    "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="          INDEX (ALL)                                               "
OK=WL(Z)
Z=")IF #TYPE NE LOB THEN )DO                                           "
OK=WL(Z)
Z="          KEYCARD FREQVAL NUMCOLS 1 COUNT 10  BOTH                  "
OK=WL(Z)
Z="                  FREQVAL NUMCOLS 2 COUNT 10  BOTH                  "
OK=WL(Z)
Z="                  FREQVAL NUMCOLS 3 COUNT 10  BOTH                  "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="          UPDATE(ALL)                                               "
OK=WL(Z)
Z=")BLANK                                                              "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z=" RUNSTATS TABLESPACE #DB..#TS.                                      "
OK=WL(Z)
Z="          SHRLEVEL CHANGE                                           "
OK=WL(Z)
Z=")IF #TYPE NE LOB THEN )DO                                           "
OK=WL(Z)
Z="          TABLE (#TB)                                               "
OK=WL(Z)
Z=")IF #USESTF = N THEN )DO                                            "
OK=WL(Z)
Z="          COLUMN (ALL)                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z=")DOT #COLLIST                                                       "
OK=WL(Z)
Z="          #RSANW                                                    "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="          INDEX (ALL)                                               "
OK=WL(Z)
Z=")IF #TYPE NE LOB THEN )DO                                           "
OK=WL(Z)
Z="          KEYCARD FREQVAL NUMCOLS 1 COUNT 10  BOTH                  "
OK=WL(Z)
Z="                  FREQVAL NUMCOLS 2 COUNT 10  BOTH                  "
OK=WL(Z)
Z="                  FREQVAL NUMCOLS 3 COUNT 10  BOTH                  "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="          UPDATE(ALL)                                               "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="// IF (RUNSTATS.RC GT 4 OR RUNSTATS.ABEND = TRUE) THEN              "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSTSPRT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSPRINT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSUDUMP DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSIN    DD  DUMMY                                                "
OK=WL(Z)
Z="//SYSTSIN  DD  *                                                    "
OK=WL(Z)
Z=" DSN SYSTEM(#SSID.)                                                 "
OK=WL(Z)
Z="     -DIS  UTILITY(RUNST.#TS.)                                      "
OK=WL(Z)
Z="     -TERM UTILITY(RUNST.#TS.)                                      "
OK=WL(Z)
Z="  END                                                               "
OK=WL(Z)
Z="// ENDIF                                                            "
OK=WL(Z)

RETURN

SKEL_SKSDMP:
Z=")DEFAULT  )#?ü<~|                                                   "
OK=WL(Z)
Z="//DELETE  EXEC  PGM=IDCAMS                                          "
OK=WL(Z)
Z="//SYSPRINT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSIN    DD  *                                                    "
OK=WL(Z)
Z=" DELETE #USER..IFCSD.TRACE                                          "
OK=WL(Z)
Z=" SET MAXCC=0                                                        "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="//******************************************************************"
OK=WL(Z)
Z="//*                                                                 "
OK=WL(Z)
Z="//* THIS IS A SKELETON OF THE JCL USED TO RUN DSN1SDMP.             "
OK=WL(Z)
Z="//* YOU MUST INSERT SDMPIN DD.                                      "
OK=WL(Z)
Z="//*                                                                 "
OK=WL(Z)
Z="//******************************************************************"
OK=WL(Z)
Z="//IFCSD EXEC PGM=IKJEFT01,DYNAMNBR=20,COND=(4,LT)                   "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSPRINT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSTSPRT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SDMPPRNT DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SDMPTRAC DD SYSOUT=*                                              "
OK=WL(Z)
Z="//*SDMPTRAC DD DISP=(NEW,CATLG,CATLG),                              "
OK=WL(Z)
Z="//* UNIT=#SYSDA,                                                    "
OK=WL(Z)
Z="//* SPACE=(CYL,(100,100)),                                          "
OK=WL(Z)
Z="//* DCB=(DSORG=PS,LRECL=32756,RECFM=VB,BLKSIZE=32760),              "
OK=WL(Z)
Z="//* DSN=#USER..IFCSD.TRACE                                          "
OK=WL(Z)
Z="//SYSUDUMP DD SYSOUT=*                                              "
OK=WL(Z)
Z="//SYSTSIN DD *                                                      "
OK=WL(Z)
Z="DSN SYSTEM(#SSID)                                                   "
OK=WL(Z)
Z="    RUN PROG(DSN1SDMP) PLAN(DSNEDCL)                                "
OK=WL(Z)
Z="END                                                                 "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="//******************************************************************"
OK=WL(Z)
Z="//*                                                                 "
OK=WL(Z)
Z="//* INSERT SDMPIN DD HERE. IT MUST BEGIN WITH A VALID               "
OK=WL(Z)
Z="//* START TRACE COMMAND (WITHOUT THE SUBSYSTEM RECOGNITION CHAR)    "
OK=WL(Z)
Z="//*                                                                 "
OK=WL(Z)
Z="//******************************************************************"
OK=WL(Z)
Z="//* (VALID SDMPIN GOES HERE)                                        "
OK=WL(Z)
Z="//*                                                                 "
OK=WL(Z)
Z="//SDMPIN DD *                                                       "
OK=WL(Z)
Z="* START ONLY IFCID 58, END SQL STATEMENT                            "
OK=WL(Z)
Z="  START TRACE=P CLASS(32) IFCID(53,58) DEST(OPX)                    "
OK=WL(Z)
Z="  FOR(1)                                                            "
OK=WL(Z)
Z="  ACTION(ABENDTER(00E60188))                                        "
OK=WL(Z)
Z="  SELECT                                                            "
OK=WL(Z)
Z="* OFFSET TO FIRST DATA SECTION CONTAINING THE SQLCA.                "
OK=WL(Z)
Z="  P4,08                                                             "
OK=WL(Z)
Z="* SQLCODE #sqlcd, RESOURCE UNAVAILABLE                              "
OK=WL(Z)
Z="  DR,74,X'#sqlcx'                                                   "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="//                                                                  "
OK=WL(Z)
Z="//XDMPIN DD *                                                       "
OK=WL(Z)
Z="--Starting a second trace                                           "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="* START ONLY IFCID 196, TIMEOUT                                     "
OK=WL(Z)
Z="START TRACE=P CLASS(32) IFCID(196) DEST(SMF)                        "
OK=WL(Z)
Z="AFTER(1)                                                            "
OK=WL(Z)
Z="* ACTION = START ACCOUNTING TRACE                                   "
OK=WL(Z)
Z="ACTION(STTRACE)                                                     "
OK=WL(Z)
Z="* FILTER ON JUST 196 RECORDS...                                     "
OK=WL(Z)
Z="SELECT                                                              "
OK=WL(Z)
Z="P4,00                                                               "
OK=WL(Z)
Z="DR,04,X'00C4'                                                       "
OK=WL(Z)
Z="* WHEN ACCOUNTING IS CUT, ABEND                                     "
OK=WL(Z)
Z="ACTION2(ABENDRET(00E60100))                                         "
OK=WL(Z)
Z="* START THE ACCOUNTING TRACE FILTER ON THE ACE OF THE AGENT         "
OK=WL(Z)
Z="* THAT TIMED OUT                                                    "
OK=WL(Z)
Z="COMMAND                                                             "
OK=WL(Z)
Z="START TRACE(ACCTG) CLASS(32) IFCID(3) DEST(SMF)                     "
OK=WL(Z)
Z="* Filter can be for ACE or EB                                       "
OK=WL(Z)
Z="FILTER(ACE)                                                         "
OK=WL(Z)
Z="--                                                                  "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="-- ABEND -904                                                       "
OK=WL(Z)
Z="START TRACE=A CLASS(32) IFCID(53,58) DEST(OPX)                      "
OK=WL(Z)
Z="FOR(1)                                                              "
OK=WL(Z)
Z="AFTER(1)                                                            "
OK=WL(Z)
Z="ACTION(ABENDRET(00E60188))                                          "
OK=WL(Z)
Z="SELECT                                                              "
OK=WL(Z)
Z="* Position to the product section                                   "
OK=WL(Z)
Z="P4,00                                                               "
OK=WL(Z)
Z="* Ensure QWHSIID = 58 or 53 (not IFCID 4)                           "
OK=WL(Z)
Z="GE,04,X'0005'                                                       "
OK=WL(Z)
Z="* Position to the data section 1                                    "
OK=WL(Z)
Z="P4,08                                                               "
OK=WL(Z)
Z="* Compare SQLCODE in QW0058SQ or QW0053SQ                           "
OK=WL(Z)
Z="DR,74,X'FFFFFC78'                                                   "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z="-- ABEND -811 (Duplicate row )                                      "
OK=WL(Z)
Z="START TRACE=P CLASS(3) RMID(22) DEST(SMF) TDATA(COR,TRA)            "
OK=WL(Z)
Z="AFTER(1)                                                            "
OK=WL(Z)
Z="FOR(1)                                                              "
OK=WL(Z)
Z="SELECT                                                              "
OK=WL(Z)
Z="* POSITION TO HEADERS (QWHS IS ALWAYS FIRST)                        "
OK=WL(Z)
Z="P4,00                                                               "
OK=WL(Z)
Z="* CHECK QWHS 01, FOR RMID 16, IFCID 58                              "
OK=WL(Z)
Z="DR,02,X'0116003A'                                                   "
OK=WL(Z)
Z="* POSITION TO SECOND SECTION (1ST DATA SECTION)                     "
OK=WL(Z)
Z="P4,08                                                               "
OK=WL(Z)
Z="* COMPARE SQLCODE FOR 811                                           "
OK=WL(Z)
Z="DR,74,X'FFFFFCD5'                                                   "
OK=WL(Z)
Z="ACTION(ABENDRET(00E60188))                                          "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="//******************************************************************"
OK=WL(Z)

RETURN

