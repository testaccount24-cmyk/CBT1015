Z=")DOT #TSLISTE                                                       "
OK=WL(Z)
Z="  REBUILD INDEX(ALL) TABLESPACE #DBNAME..#TSNAME.                   "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z="  REBUILD INDEX(ALL) TABLESPACE #OBJECT.                            "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="                                                                    "
OK=WL(Z)
Z=")SEL #AUSWKZ  = IX                                                  "
OK=WL(Z)
Z="//SYSIN DD *                                                        "
OK=WL(Z)
Z="--OPTIONS PREVIEW                                                   "
OK=WL(Z)
Z="--OPTIONS EVENT(ITEMERROR,SKIP)                                     "
OK=WL(Z)
Z=")IF #USEDOT  = Y THEN )DO                                           "
OK=WL(Z)
Z=")DOT #TSLISTE                                                       "
OK=WL(Z)
Z="  REBUILD INDEX(#Creator..#Name.)                                   "
OK=WL(Z)
Z=")ENDDOT                                                             "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ELSE )DO                                                           "
OK=WL(Z)
Z="  REBUILD INDEX(#OBJECT.)                                           "
OK=WL(Z)
Z=")ENDDO                                                              "
OK=WL(Z)
Z=")ENDSEL                                                             "
OK=WL(Z)
Z="/*                                                                  "
OK=WL(Z)
Z="// IF (REBUILD.RC GT 4 OR REBUILD.ABEND = TRUE) THEN                "
OK=WL(Z)
Z="//TERM    EXEC PGM=IKJEFT01,DYNAMNBR=20                             "
OK=WL(Z)
Z=")IM SKSTEPLI                                                        "
OK=WL(Z)
Z="//SYSTSPRT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSPRINT DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSUDUMP DD  SYSOUT=*                                             "
OK=WL(Z)
Z="//SYSIN    DD  DUMMY                                                "
OK=WL(Z)
Z="//SYSTSIN  DD  *                                                    "
OK=WL(Z)
