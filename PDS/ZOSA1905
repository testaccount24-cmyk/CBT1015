SKEL_SKMODALL:
Z=")DEFAULT  )#?ü<~|                                               "
OK=WL(Z)
Z="//****************************************************          "
OK=WL(Z)
Z="//* MODIFY RECOVERY                                             "
OK=WL(Z)
Z="//****************************************************          "
OK=WL(Z)
Z="//*                                                             "
OK=WL(Z)
Z="//GENMODST EXEC PGM=IKJEFT01                                    "
OK=WL(Z)
Z=")IM SKSTEPLI                                                    "
OK=WL(Z)
Z="//SYSEXEC  DD DISP=SHR,DSN=#PROCLIB                             "
OK=WL(Z)
Z="//SYSTSPRT DD SYSOUT=*                                          "
OK=WL(Z)
Z="//OUTDD    DD  DISP=(,PASS),                                    "
OK=WL(Z)
Z="//             SPACE=(CYL,(3,5)),                               "
OK=WL(Z)
Z="//             UNIT=#SYSDA,                                     "
OK=WL(Z)
Z="//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=3120),            "
OK=WL(Z)
Z="//             DSN=&&MODREC                                     "
OK=WL(Z)
Z="//RXPARM   DD *                                                 "
OK=WL(Z)
Z="SSID          = '#SSID'        /* SSID DES DB2SYSTEMS           " ||,
"    */          "
OK=WL(Z)
Z="LOCATION      = ''             /* LOCATION ZU ANDERER DB2 DB(LUW" ||,
")   */          "
OK=WL(Z)
Z="SQLTERM       = ';'            /* SQL TERMINATION               " ||,
"    */          "
OK=WL(Z)
Z="OUTPUT-FORMAT = 'FIXED'        /* FEST BREITE                   " ||,
"    */          "
OK=WL(Z)
Z="OUTPUT-FORMAT = 'CSV'          /* SEMICOLON GETRENNT            " ||,
"    */          "
OK=WL(Z)
Z="OUTPUT-FORMAT = 'CSH'          /* SEMICOLON GETRENNT mit Header " ||,
"    */          "
OK=WL(Z)
Z="OUTPUT-FORMAT = 'VARIABLE'     /* VARIABLE BREITE (REXX WORD)   " ||,
"    */          "
OK=WL(Z)
Z="OUTPUT-FORMAT = 'WORDWRAP'     /* ZEILENUMBRUCH BEI JEDEM COLUMN" ||,
"    */          "
OK=WL(Z)
Z="COLDEL        = ';'            /* CSV COL  DELIMITER            " ||,
"    */          "
OK=WL(Z)
Z="CHARDEL       = '"'"'"'        /* CSV CHAR DELIMITER            " ||,
"    */          "
OK=WL(Z)
Z="AUTO_COMMIT   = 'NO'           /* AUTOCOMMIT NACH JEDEM STATEMEN" ||,
"T   */          "
OK=WL(Z)
Z="/*                                                              "
OK=WL(Z)
Z="//SYSPRINT DD SYSOUT=*                                          "
OK=WL(Z)
Z="//SYSTSIN  DD *                                                 "
OK=WL(Z)
Z="  EXECUTIL SEARCHDD(YES)                                        "
OK=WL(Z)
Z="  %DYNAMSQL                                                     "
OK=WL(Z)
Z="/*                                                              "
OK=WL(Z)
Z="//SYSIN    DD *                                                 "
OK=WL(Z)
Z="WITH MODTAB  (RN, PN, DBNAME, TSNAME                            "
OK=WL(Z)
Z="                             , DSNUM, ICTYPE, FILESEQNO         "
OK=WL(Z)
Z="                             , DEVTYPE, DSNAME, ICDATE          "
OK=WL(Z)
Z="                             , TIMESTAMP, DSVOLSER              "
OK=WL(Z)
Z="                            )                                   "
OK=WL(Z)
Z="         AS (SELECT ROWNUMBER()                                 "
OK=WL(Z)
Z="                    OVER(ORDER     BY DBNAME                    "
OK=WL(Z)
Z="                                    , TSNAME                    "
OK=WL(Z)
Z="                                    , TIMESTAMP DESC) AS RN     "
OK=WL(Z)
Z="                 ,  ROWNUMBER()                                 "
OK=WL(Z)
Z="                    OVER(PARTITION BY DBNAME                    "
OK=WL(Z)
Z="                                    , TSNAME) AS PN             "
OK=WL(Z)
Z="                 ,  DBNAME                                      "
OK=WL(Z)
Z="                 ,  TSNAME                                      "
OK=WL(Z)
Z="                 ,  DSNUM                                       "
OK=WL(Z)
Z="                 ,  ICTYPE                                      "
OK=WL(Z)
Z="                 ,  FILESEQNO                                   "
OK=WL(Z)
Z="                 ,  DEVTYPE                                     "
OK=WL(Z)
Z="                 ,  DSNAME                                      "
OK=WL(Z)
Z="                 ,  '20' CONCAT CHAR(ICDATE) AS ICDATE          "
OK=WL(Z)
Z="                 ,  TIMESTAMP                                   "
OK=WL(Z)
Z="                 ,  DSVOLSER                                    "
OK=WL(Z)
Z="             FROM SYSIBM.SYSCOPY                                "
OK=WL(Z)
Z="             WHERE ICTYPE = 'F'                                 "
OK=WL(Z)
Z="             )                                                  "
OK=WL(Z)
Z="SELECT '   MODIFY RECOVERY TABLESPACE '                         "
OK=WL(Z)
Z="CONCAT RTRIM(DBNAME) CONCAT '.' CONCAT RTRIM(TSNAME)            "
OK=WL(Z)
Z="    ,  '          DELETE DATE(' CONCAT ICDATE CONCAT ')'        "
OK=WL(Z)
Z="    ,  '          '                                             "
OK=WL(Z)
Z="FROM MODTAB                                                     "
OK=WL(Z)
Z="WHERE PN = 3                                                    "
OK=WL(Z)
Z="AND   DBNAME = 'XXXXXXXX'                                       "
OK=WL(Z)
Z="AND   TSNAME = 'XXXXXXXX'                                       "
OK=WL(Z)
Z="ORDER BY DBNAME, TSNAME, PN DESC                                "
OK=WL(Z)
Z="FOR FETCH ONLY  WITH UR                                         "
OK=WL(Z)
Z=" ;                                                              "
OK=WL(Z)
Z="//*                                                             "
OK=WL(Z)
Z="//MODIFY   EXEC PGM=DSNUTILB,PARM=(#SSID,'REP.MODREC')          "
OK=WL(Z)
Z=")IM SKSTEPLI                                                    "
OK=WL(Z)
Z="//SYSPRINT  DD  DISP=(,PASS),                                   "
OK=WL(Z)
Z="//             SPACE=(CYL,(3,5)),                               "
OK=WL(Z)
Z="//             UNIT=#SYSDA,                                     "
OK=WL(Z)
Z="//             DCB=(RECFM=FB,LRECL=133,BLKSIZE=0),              "
OK=WL(Z)
Z="//             DSN=&&DELCOPY                                    "
OK=WL(Z)
Z="//UTPRINT   DD SYSOUT=*                                         "
OK=WL(Z)
Z="//SYSIN     DD DISP=(OLD,PASS),DSN=&&MODREC                     "
OK=WL(Z)
Z="//*                                                             "
OK=WL(Z)
Z="//GENDEL    EXEC PGM=IKJEFT01                                   "
OK=WL(Z)
Z="//SYSEXEC  DD DISP=SHR,DSN=#PROCLIB                             "
OK=WL(Z)
Z="//SYSPRINT  DD  SYSOUT=*                                        "
OK=WL(Z)
Z="//SYSTSPRT  DD  SYSOUT=*                                        "
OK=WL(Z)
Z="//SYSOUT    DD  SYSOUT=*                                        "
OK=WL(Z)
Z="//MODIFID   DD  DISP=(OLD,PASS),DSN=&&DELCOPY                   "
OK=WL(Z)
Z="//DELFILE   DD  DISP=(,PASS),                                   "
OK=WL(Z)
Z="//             SPACE=(CYL,(3,5)),                               "
OK=WL(Z)
Z="//             UNIT=#SYSDA,                                     "
OK=WL(Z)
Z="//             DCB=(RECFM=FB,LRECL=133,BLKSIZE=0),              "
OK=WL(Z)
Z="//             DSN=&&DELFILE                                    "
OK=WL(Z)
Z="//SYSTSIN   DD  *                                               "
OK=WL(Z)
Z="  EXECUTIL SEARCHDD(YES)                                        "
OK=WL(Z)
Z="  %RXMODDEL                                                     "
OK=WL(Z)
Z="//*                                                             "
OK=WL(Z)
Z="//SYSIN     DD  DUMMY                                           "
OK=WL(Z)
Z="/*                                                              "
OK=WL(Z)
Z="//*                                                             "
OK=WL(Z)
Z="//* EXECUTE  DELETE STATEMENTS                                  "
OK=WL(Z)
Z="//*                                                             "
OK=WL(Z)
Z="//DELETE   EXEC PGM=IDCAMS,COND=(EVEN)                          "
OK=WL(Z)
Z="//SYSPRINT  DD SYSOUT=*                                         "
OK=WL(Z)
Z="//SYSIN     DD  DISP=(OLD,PASS),DSN=&&DELFILE                   "
OK=WL(Z)
Z="//          DD  *                                               "
OK=WL(Z)
Z="   SET MAXCC = 0                                                "
OK=WL(Z)
Z="//*                                                             "
OK=WL(Z)

